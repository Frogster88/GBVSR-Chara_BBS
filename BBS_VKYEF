beginState: s32'dmy'
  sprite: s32'null', 1
endState: 

beginSubroutine: s32'MeshSet_VKN'
  linkPawnWithAnimation: s32'VKN', s32''
  beginMeshSet: s32'VKN'
    addToMeshSet: s32'body'
  endMeshSet: 
  swapMeshSet: s32'VKN'
endSubroutine: 

beginSubroutine: s32'MeshSet_VKN_stand'
  linkPawnWithAnimation: s32'VKN_stand', s32''
