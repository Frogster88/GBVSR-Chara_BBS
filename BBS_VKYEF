beginSubroutine: s32'MeshSet_VKDMP'
  ifOpponentCharacterMain: s16'vky'
  if: Mem(Tmp)
    if: Mem(SideID)
      linkPawnWithAnimation: s32'VKDMP_2P', s32''
      beginMeshSet: s32'VKDMP_2P'
        addToMeshSet: s32'dormouthMP'
      endMeshSet: 
      swapMeshSet: s32'VKDMP_2P'
    endIf: 
    else: 
      linkPawnWithAnimation: s32'VKDMP_1P', s32''
      beginMeshSet: s32'VKDMP_1P'
        addToMeshSet: s32'dormouthMP'
      endMeshSet: 
      swapMeshSet: s32'VKDMP_1P'
    endElse: 
  endIf: 
  else: 
    linkPawnWithAnimation: s32'VKDMP_1P', s32''
    beginMeshSet: s32'VKDMP_1P'
      addToMeshSet: s32'dormouthMP'
    endMeshSet: 
    swapMeshSet: s32'VKDMP_1P'
  endElse: 
endSubroutine: 

