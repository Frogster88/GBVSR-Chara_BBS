beginState: s32'dmy'
    sprite: s32'null', 1
endState: 

beginSubroutine: s32'Box_Start'
    linkPawnWithAnimation: s32'Box', s32''
    setNoCollision: 0
    checkScreenCollision: 0
    worldCollision: 0
    invulnForce: 1
    ignoreSuperFreezeFromAlly: 1
endSubroutine: 

beginSubroutine: s32'Box_End'
    linkPawnWithAnimation: s32'', s32''
    deactivateCurrentObject: 
endSubroutine: 

beginState: s32'Box_cha800cs'
    upon: (IMMEDIATE)
        callSubroutine: s32'Box_Start'
        setLinkObjectDestroyOnStateChange: 2
    endUpon: 
    sprite: s32'null', 2147483647
    playAnimation: s32'cha800cs', 1
    endSprite: 
    exitState: 
endState: 

