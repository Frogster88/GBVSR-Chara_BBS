beginSubroutine: s32'MeshSet_Limited_sabun_Setting'
    if: Mem(PlayerVal0)
        swapMeshSet: s32'limited_sabun'
        setDefaultMeshSet: s32'limited_sabun'
        upon: (BEFORE_EXIT)
            swapMeshSet: s32'limited'
            setDefaultMeshSet: s32'limited'
        endUpon: 
    endIf: 
endSubroutine: 

beginSubroutine: s32'OnFrameStep'
    callSubroutine: s32'cmnTest'
    if: Mem(PlayerVal0)
        counterHitFrozenTime: 0, 1250
    endIf: 
endSubroutine: 

beginSubroutine: s32'OnIdling'
    if: Mem(PlayerVal0)
        ifNot: Mem(DebugSkillEx)
            ifNot: Mem(295)
                ifNot: Mem(DebugSkillEx)
                    if: Mem(BattleDoing)
                        ifOperation: (IS_GREATER_OR_EQUAL), Mem(ObjReg0), Val(12)
                            createParticleWithArg2: s32'vir_HenshinAura', (PELVIS_3D)
                            storeValue: Mem(ObjReg0), Val(0)
                        endIf: 
                        else: 
                            modifyVar: (ADD), Mem(ObjReg0), Val(1)
                        endElse: 
                    endIf: 
                endIf: 
            endIf: 
        endIf: 
    endIf: 
    if: Mem(ObjReg5)
        modifyVar: (ADD), Mem(ObjReg5), Val(-1)
        ifNot: Mem(ObjReg5)
            Unknown413: 0, 1, 7, 0
        endIf: 
    endIf: 
    if: Mem(IsLand)
        storeValue: Mem(PlayerVal1), Val(0)
    endIf: 
    ifOperation: (IS_EQUAL), Mem(ActionTime), Val(3)
        callSubroutine: s32'AbilityCoolDown_N'
        callSubroutine: s32'AbilityCoolDown_6'
        callSubroutine: s32'AbilityCoolDown_4'
        callSubroutine: s32'AbilityCoolDown_2'
        callSubroutine: s32'AbilityCoolDown_2_ON_FINALIZE'
    endIf: 
endSubroutine: 

beginSubroutine: s32'OnActionBegin'
    callSubroutine: s32'cmn_FN_OnActionBegin'
    checkSomething: 3, 3
    if: Mem(Tmp)
        Unknown1924: 0, 1
        Unknown1924: 1, 1
        Unknown1924: 2, 1
        Unknown1924: 3, 1
    endIf: 
    else: 
        Unknown1924: 0, 0
        Unknown1924: 1, 0
        Unknown1924: 2, 0
        Unknown1924: 3, 0
    endElse: 
    checkCurrentStateName: (PLAYER), s32'CmnActLockWait'
    if: Mem(Tmp)
        if: Mem(PlayerVal0)
            swapMeshSet: s32'limited'
            setDefaultMeshSet: s32'limited'
        endIf: 
    endIf: 
endSubroutine: 

beginSubroutine: s32'OnFinalize'
    callSubroutine: s32'cmn_FN_OnFinalize'
    Unknown413: 0, 1, 11, 0
    haikuEnd: 
endSubroutine: 

beginSubroutine: s32'OnDamage'
    callSubroutine: s32'cmn_Fn_OnDamage'
    ifOperation: (IS_LESSER_OR_EQUAL), Mem(ActionTime), Val(2)
        callSubroutine: s32'AbilityCoolDown_N'
        callSubroutine: s32'AbilityCoolDown_6'
        callSubroutine: s32'AbilityCoolDown_4'
        callSubroutine: s32'AbilityCoolDown_2_DAMAGE'
    endIf: 
endSubroutine: 

beginSubroutine: s32'OnLanding'
    callSubroutine: s32'cmn_FN_OnLanding'
endSubroutine: 

beginSubroutine: s32'AbilityCoolDown_2_DAMAGE'
    checkCurrentStateName: (SELF), s32'SpecialChevalier_A'
    if: Mem(Tmp)
        storeValue: Mem(289), Val(20)
        Unknown1923: 2
    endIf: 
    checkCurrentStateName: (SELF), s32'SpecialChevalier_B'
    if: Mem(Tmp)
        storeValue: Mem(289), Val(20)
        Unknown1923: 2
    endIf: 
    checkCurrentStateName: (SELF), s32'SpecialChevalier_C'
    if: Mem(Tmp)
        storeValue: Mem(289), Val(420)
        if: Mem(PlayerVal0)
            storeValue: Mem(289), Val(390)
        endIf: 
        Unknown1923: 2
    endIf: 
    checkCurrentStateName: (SELF), s32'SpecialChevalier_D'
    if: Mem(Tmp)
        if: Mem(289)
            modifyVar2: (ADD), Val(180), Mem(289)
            storeValue: Mem(289), Mem(Tmp)
        endIf: 
        else: 
            storeValue: Mem(289), Val(180)
        endElse: 
        Unknown1923: 2
    endIf: 
endSubroutine: 

beginSubroutine: s32'AbilityCoolDown_2_ON_FINALIZE'
    checkCurrentStateName: (SELF), s32'SpecialChevalier_A'
    if: Mem(Tmp)
        upon: (BEFORE_EXIT)
            storeValue: Mem(289), Val(20)
            Unknown1923: 2
        endUpon: 
    endIf: 
    checkCurrentStateName: (SELF), s32'SpecialChevalier_B'
    if: Mem(Tmp)
        upon: (BEFORE_EXIT)
            storeValue: Mem(289), Val(20)
            Unknown1923: 2
        endUpon: 
    endIf: 
    checkCurrentStateName: (SELF), s32'SpecialChevalier_C'
    if: Mem(Tmp)
        upon: (BEFORE_EXIT)
            storeValue: Mem(289), Val(420)
            if: Mem(PlayerVal0)
                storeValue: Mem(289), Val(390)
            endIf: 
            Unknown1923: 2
        endUpon: 
    endIf: 
    checkCurrentStateName: (SELF), s32'SpecialChevalier_D'
    if: Mem(Tmp)
        upon: (BEFORE_EXIT)
            if: Mem(ObjReg6)
                modifyVar2: (ADD), Val(180), Mem(289)
                storeValue: Mem(289), Mem(Tmp)
            endIf: 
            else: 
                storeValue: Mem(289), Val(180)
            endElse: 
            Unknown1923: 2
        endUpon: 
    endIf: 
endSubroutine: 

beginSubroutine: s32'AbilityCoolDown_N'
