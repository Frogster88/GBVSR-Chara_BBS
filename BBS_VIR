        Unknown1923: 1
    endIf: 
    checkCurrentStateName: (SELF), s32'AntiAir_B'
    if: Mem(Tmp)
        Unknown1921: 1, 240
        Unknown1923: 1
    endIf: 
    checkCurrentStateName: (SELF), s32'AntiAir_C'
    if: Mem(Tmp)
        Unknown1921: 1, 600
        if: Mem(PlayerVal0)
            Unknown1921: 1, 570
        endIf: 
        Unknown1923: 1
