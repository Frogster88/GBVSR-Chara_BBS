    if: Mem(Tmp)
        Unknown1921: 0, 20
        Unknown1923: 0
    endIf: 
    checkCurrentStateName: (SELF), s32'SpecialMultiAttack_B'
    if: Mem(Tmp)
        Unknown1921: 0, 20
        Unknown1923: 0
    endIf: 
    checkCurrentStateName: (SELF), s32'SpecialMultiAttack_C'
    if: Mem(Tmp)
        Unknown1921: 0, 420
        if: Mem(PlayerVal0)
            Unknown1921: 0, 390
        endIf: 
        Unknown1923: 0
    endIf: 
    checkCurrentStateName: (SELF), s32'SpecialMultiAttack_D'
    if: Mem(Tmp)
        if: Mem(287)
            storeValue: Mem(417), Val(1)
        endIf: 
        modifyVar2: (ADD), Val(180), Mem(287)
        storeValue: Mem(287), Mem(Tmp)
        Unknown1923: 0
    endIf: 
endSubroutine: 

beginSubroutine: s32'AbilityCoolDown_6'
