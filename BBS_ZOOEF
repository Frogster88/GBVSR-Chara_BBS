beginSubroutine: s32'DYR_Start'
  linkPawnWithAnimation: s32'DYR', s32''
  beginMeshSet: s32'dyr'
    addToMeshSet: s32'body'
  endMeshSet: 
  swapMeshSet: s32'dyr'
  setNoCollision: 0
  checkScreenCollision: 0
  worldCollision: 0
  invulnForce: 1
  ignoreSuperFreezeFromAlly: 1
endSubroutine: 

beginSubroutine: s32'DYR_End'
  linkPawnWithAnimation: s32'', s32''
  deactivateCurrentObject: 
endSubroutine: 

