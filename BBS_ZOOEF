beginState: s32'ZOO_400_Thunder_log'
  upon: (IMMEDIATE)
    worldCollision: 0
    storeValue: Mem(PlayerVal0), Val(1)
    callSubroutine: s32'Thunder_SetColor'
    specialAttack: 
    callSubroutine: s32'cmnNormalAtkLv3_Land_AtkInit'
    damage: 700
    chipDamagePercent: 20
    blockstunAmount: 14
    damageEffect: 2, s32'ZOO_SPHitEff'
    attackIgnoreCaution: 1
    hitstop: 3
    enemyHitstopAddition: 0, 0, 2
    hitAirPushbackX: 3000
    hitAirPushbackY: -30000
    isAirUnblockable: 0
    disablePunish: 1
    guardEffectEconomy: 1
    initializeDestroy: 
    destroyOnDamageCollision: 1
    Unknown2207: 1
    Unknown2208: 1
    Unknown2209: 1
    Unknown2257: 1
    Unknown2212: 1
    Unknown2213: 1
    numberOfHits: 1
    upon: (ON_DESTROY)
      clearUpon: (FRAME_STEP)
      storeValue: Mem(ActReg2), Val(0)
      storeValue: Mem(ActReg3), Val(0)
      storeValue: Mem(ActReg4), Val(0)
      gotoLabelRequests: s32'SignEnd'
    endUpon: 
    upon: (FRAME_STEP)
      ifNot: Mem(BattleDoing)
        clearUpon: (FRAME_STEP)
        gotoLabelRequests: s32'SignEnd'
      endIf: 
      if: Mem(ActReg2)
        modifyVar: (ADD), Mem(ActReg2), Val(1)
        ifOperation: (IS_GREATER_OR_EQUAL), Mem(ActReg2), Val(9)
          privateSE: s32'zoo_20'
          storeValue: Mem(ActReg2), Val(1)
        endIf: 
      endIf: 
      if: Mem(ActReg3)
        checkInput: (INPUT_HOLD_L)
        ifNot: Mem(Tmp)
          checkInput: (INPUT_HOLD_M)
          ifNot: Mem(Tmp)
            checkInput: (INPUT_HOLD_H)
            ifNot: Mem(Tmp)
              checkInput: 25
              ifNot: Mem(Tmp)
                storeValue: Mem(ActReg3), Val(0)
                storeValue: Mem(ActReg4), Val(10)
              endIf: 
            endIf: 
          endIf: 
        endIf: 
      endIf: 
      if: Mem(ActReg4)
        modifyVar: (ADD), Mem(ActReg4), Val(-1)
        ifOperation: (IS_LESSER_OR_EQUAL), Mem(ActReg4), Val(0)
          clearUpon: (FRAME_STEP)
          storeValue: Mem(ActReg2), Val(0)
          storeValue: Mem(ActReg3), Val(0)
          storeValue: Mem(ActReg4), Val(0)
          gotoLabelRequests: s32'ThunderStart'
        endIf: 
      endIf: 
    endUpon: 
    upon: (BEFORE_EXIT)
      Unknown1925: 2, 1
      storeValue: Mem(PlayerVal0), Val(0)
    endUpon: 
  endUpon: 
  sprite: s32'null', 1
  ifNot: Mem(SideID)
    linkParticle: s32'zoo_400_SignLoop'
  endIf: 
  else: 
    linkParticle: s32'zoo_400_2PSignLoop'
  endElse: 
  privateSE: s32'zoo_20'
  storeValue: Mem(ActReg2), Val(1)
  sprite: s32'null', 23
  ifOperation: (IS_GREATER), Mem(PosX), Val(1600000)
    deactivateObjectById: (SELF)
  endIf: 
  ifOperation: (IS_LESSER), Mem(PosX), Val(-1600000)
    deactivateObjectById: (SELF)
  endIf: 
  sprite: s32'null', 70
  storeValue: Mem(ActReg3), Val(1)
  beginLabel: s32'ThunderStart'
  sprite: s32'null', 1
  clearUpon: (FRAME_STEP)
  sprite: s32'null', 8
  ifNot: Mem(SideID)
    linkParticle: s32'zoo_400_SignEnd'
  endIf: 
  else: 
    linkParticle: s32'zoo_400_2PSignEnd'
  endElse: 
  clearUpon: (ON_DESTROY)
  privateSE: s32'zoo_08'
  createObject: s32'ZOO_400_ThunderChild', (PLAYER)
  stackObject: (STACK0), (PREVIOUS)
  upon: 35
    deactivateObjectById: (SELF)
    recoveryState: 
  endUpon: 
  upon: 38
    deactivateObjectById: (SELF)
    recoveryState: 
  endUpon: 
  sprite: s32'ZOO_LandThunder_Atk', 1
  hit: 
  clearUpon: 35
  clearUpon: 38
  sprite: s32'ZOO_LandThunder_Atk', 5
  Unknown1793: 4, 24
  sprite: s32'null', 20
  Unknown1925: 2, 1
  storeValue: Mem(PlayerVal0), Val(0)
  endSprite: 
  exitState: 
