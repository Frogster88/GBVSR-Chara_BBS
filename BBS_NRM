beginSubroutine: s32'Slash_Kagura_Init'
  storeValue: Mem(PlayerVal0), Val(1)
  callSubroutine: s32'Assault_Genji_CancelInit'
  if: Mem(ActReg0)
    callSubroutine: s32'KamaeCheck'
    playerActionInitializeByCategory: 17, 1
    Unknown2033: 10000
    faceOpponent: 
    Unknown110: 
