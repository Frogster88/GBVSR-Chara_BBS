    enableJump: 1
    enableSpecials: 1
    callSubroutine: s32'NandemoLandNormalAttack'
    sprite: s32'nrm208_02', 3
    callSubroutine: s32'KamaeCheck'
    sprite: s32'nrm208_03', 3
    sprite: s32'nrm208_04', 3
    sprite: s32'nrm208_05', 3
    sprite: s32'nrm208_06', 3
    sprite: s32'nrm208_07', 3
    sprite: s32'nrm208_08', 3
    sprite: s32'nrm208_09', 3
    endSprite: 
    exitState: 
