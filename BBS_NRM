beginState: s32'NmlAtk5D'
    upon: (IMMEDIATE)
        attackIgnoreCaution: 1
        attackEndDelay: 
        haltMomentum: 1
        setEventTrigger: 13, 11
        upon: 13
            clearUpon: 13
            callSubroutine: s32'cmnNandemoCancel'
            enableWalkForward: 0
            enableWalkBack: 0
            enableCrouch: 0
            callSubroutine: s32'KamaeCheck'
        endUpon: 
    endUpon: 
    gotoIfOperation: s32'Genji_Ikou', (IS_EQUAL), Mem(PlayerVal0), Val(1)
    beginLabel: s32'Kagura_Ikou'
    sprite: s32'nrm208_00', 2
    sprite: s32'nrm208_01', 3
