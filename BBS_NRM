beginState: s32'NmlAtkThrow'
  upon: (IMMEDIATE)
    callSubroutine: s32'cmnLandThrowInit'
    checkLastStateName: s32'Assault_Genji_A'
    if: Mem(Tmp)
      storeValue: Mem(PlayerVal0), Val(0)
      callSubroutine: s32'KamaeCheck'
    endIf: 
    else: 
      checkLastStateName: s32'Assault_Genji_B'
      if: Mem(Tmp)
        storeValue: Mem(PlayerVal0), Val(0)
        callSubroutine: s32'KamaeCheck'
        faceOpponent: 
      endIf: 
      else: 
        checkLastStateName: s32'Assault_Genji_C'
        if: Mem(Tmp)
          storeValue: Mem(PlayerVal0), Val(0)
          callSubroutine: s32'KamaeCheck'
          faceOpponent: 
        endIf: 
      endElse: 
    endElse: 
  endUpon: 
  sprite: s32'nrm300_00', 1
  sprite: s32'nrm300_01', 1
  sprite: s32'nrm300_02', 1
  callSubroutine: s32'HoldCheck_Init'
  cmnSfx: s32'SE_BTL_SwingBlowM'
  sprite: s32'nrm300_03', 3
  hit: 
