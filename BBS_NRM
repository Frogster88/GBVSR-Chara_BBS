beginSubroutine: s32'KamaeCheck'
  if: Mem(PlayerVal0)
    setDefaultMeshSet: s32'Kagura'
    setDamageSprite: 21, s32'nrm041_04'
    setDamageSprite: 22, s32'nrm041_04'
  endIf: 
  else: 
    setDefaultMeshSet: s32'Genji'
    setDamageSprite: 21, s32'nrm040_04'
    setDamageSprite: 22, s32'nrm040_04'
  endElse: 
endSubroutine: 

beginSubroutine: s32'HoldCheckInput_Init'
  ifNot: Mem(DebugSkillEx)
    ifOperation: (IS_GREATER_OR_EQUAL), Mem(ActionTime), Val(4)
      checkInput: 20
      if: Mem(Tmp)
        storeValue: Mem(ActReg6), Val(1)
      endIf: 
      checkInput: 122
      if: Mem(Tmp)
        storeValue: Mem(ActReg6), Val(0)
        storeValue: Mem(ActReg7), Val(0)
      endIf: 
      checkInput: 121
      if: Mem(Tmp)
        storeValue: Mem(ActReg6), Val(0)
        storeValue: Mem(ActReg7), Val(0)
      endIf: 
      checkInput: 152
      if: Mem(Tmp)
        storeValue: Mem(ActReg6), Val(0)
        storeValue: Mem(ActReg7), Val(0)
      endIf: 
      checkInput: 155
      if: Mem(Tmp)
        storeValue: Mem(ActReg6), Val(0)
        storeValue: Mem(ActReg7), Val(0)
      endIf: 
      checkInput: (INPUT_PRESS_U)
      if: Mem(Tmp)
        checkInput: (INPUT_236)
        if: Mem(Tmp)
          storeValue: Mem(ActReg6), Val(0)
          storeValue: Mem(ActReg7), Val(0)
        endIf: 
        checkInput: (INPUT_41236)
        if: Mem(Tmp)
          storeValue: Mem(ActReg6), Val(0)
          storeValue: Mem(ActReg7), Val(0)
        endIf: 
        checkInput: (INPUT_623)
        if: Mem(Tmp)
          storeValue: Mem(ActReg6), Val(0)
          storeValue: Mem(ActReg7), Val(0)
        endIf: 
        checkInput: (INPUT_214)
        if: Mem(Tmp)
          storeValue: Mem(ActReg6), Val(0)
          storeValue: Mem(ActReg7), Val(0)
        endIf: 
      endIf: 
      checkCurrentStateName: (SELF), s32'Slash_Genji_A'
      ifNot: Mem(Tmp)
        checkCurrentStateName: (SELF), s32'Slash_Genji_B'
        ifNot: Mem(Tmp)
          checkCurrentStateName: (SELF), s32'Slash_Genji_C'
          ifNot: Mem(Tmp)
            upon: 93
              clearUpon: 36
              storeValue: Mem(ActReg6), Val(0)
              storeValue: Mem(ActReg7), Val(0)
            endUpon: 
          endIf: 
        endIf: 
      endIf: 
      if: Mem(ActReg6)
        modifyVar: (ADD), Mem(ActReg7), Val(1)
        ifOperation: (IS_GREATER_OR_EQUAL), Mem(ActReg7), Val(5)
          storeValue: Mem(PlayerVal3), Val(1)
          clearUpon: 36
          if: Mem(PlayerVal0)
            exPointFReset: (CENTER), 0, 50000
            createParticle: s32'nrm_genjiIcon', (EX_POINT_F)
            privateSE: s32'nrm_15'
            createObject: s32'Trial_genji', (PLAYER)
          endIf: 
          else: 
            exPointFReset: (CENTER), 0, 50000
            createParticle: s32'nrm_kaguraIcon', (EX_POINT_F)
            privateSE: s32'nrm_15'
            createObject: s32'Trial_kagura', (PLAYER)
          endElse: 
          isObjectActive: (STACK0)
          if: Mem(Tmp)
            deactivateObjectById: (STACK0)
          endIf: 
          createObject: s32'UsedKanri', (PLAYER)
          stackObject: (STACK0), (PREVIOUS)
        endIf: 
      endIf: 
    endIf: 
  endIf: 
endSubroutine: 

