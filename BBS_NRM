beginSubroutine: s32'NandemoLandNormalAttack'
  enableWhiffCancel: 1
  addWhiffCancelOption: s32'NmlAtk5ANear'
  addWhiffCancelOption: s32'NmlAtk5BNear'
  addWhiffCancelOption: s32'NmlAtk5CNear'
  addWhiffCancelOption: s32'NmlAtk5BFar'
  addWhiffCancelOption: s32'NmlAtk5CFar'
  addWhiffCancelOption: s32'NmlAtk2A'
  addWhiffCancelOption: s32'NmlAtk2B'
  addWhiffCancelOption: s32'NmlAtk2C'
  addWhiffCancelOption: s32'NmlAtk2D'
  addWhiffCancelOption: s32'NmlAtkThrow'
  addWhiffCancelOption: s32'NmlAtkBackThrow'
endSubroutine: 

beginState: s32'NmlAtk5D'
  upon: (IMMEDIATE)
    attackIgnoreCaution: 1
    attackEndDelay: 
    haltMomentum: 1
    setEventTrigger: 13, 11
    upon: 13
      clearUpon: 13
      callSubroutine: s32'cmnNandemoCancel'
      enableWalkForward: 0
      enableWalkBack: 0
      enableCrouch: 0
      callSubroutine: s32'KamaeCheck'
    endUpon: 
  endUpon: 
  gotoIfOperation: s32'Genji_Ikou', (IS_EQUAL), Mem(PlayerVal0), Val(1)
  beginLabel: s32'Kagura_Ikou'
  sprite: s32'nrm208_00', 2
  sprite: s32'nrm208_01', 3
  modifyVar: (ADD), Mem(PlayerVal0), Val(1)
  exPointFReset: (CENTER), 0, 50000
  createParticle: s32'nrm_kaguraIcon', (EX_POINT_F)
  characterSfx: s32'nrm_01'
  characterSfx: s32'nrm_04_b'
  sprite: s32'nrm208_01', 1
  enableJump: 1
  enableSpecials: 1
  callSubroutine: s32'NandemoLandNormalAttack'
  sprite: s32'nrm208_02', 3
  callSubroutine: s32'KamaeCheck'
  sprite: s32'nrm208_03', 3
  sprite: s32'nrm208_04', 3
  sprite: s32'nrm208_05', 3
