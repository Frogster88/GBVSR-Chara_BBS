        callSubroutine: s32'KamaeCheck'
        playerActionInitializeByCategory: 17, 1
        faceOpponent: 
        popVelocityX: 
        velocityXPercentEachFrame: 80
        ifOperation: (IS_LESSER_OR_EQUAL), Mem(SpeedX), Val(0)
            velocityXPercent: 0
        endIf: 
    endIf: 
    checkCurrentStateName: (SELF), s32'AntiAir_Kagura_C'
    if: Mem(Tmp)
        callSubroutine: s32'cmnSpecialAttackC_Init'
        if: Mem(ActReg0)
            clearUpon: 116
            callSubroutine: s32'SpecialAttackC_Init_NRM'
        endIf: 
    endIf: 
    else: 
        callSubroutine: s32'cmnSpecialAttack_Init'
        if: Mem(ActReg0)
            clearUpon: 116
        endIf: 
    endElse: 
    attackIgnoreCaution: 1
    groundHitEffect: (AIR_LAUNCH)
    airHitEffect: (AIR_LAUNCH)
    hitAirPushbackX: 3000
    hitAirPushbackY: 28000
    hitEffectSlash: 1
    callSubroutine: s32'HoldCheck_Init'
