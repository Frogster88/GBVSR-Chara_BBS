    callSubroutine: s32'402_SpeedEff_Mateme'
    sendSignalToAction: s32'nrm_402_SpeedEff', (SIGNAL_A)
  endIf: 
  checkCurrentStateName: (SELF), s32'Slash_Kagura_C'
  if: Mem(Tmp)
    callSubroutine: s32'cmnSpecialAtkLv5_AtkInit'
    callSubroutine: s32'cmnSpecialAttackC_Init'
    if: Mem(ActReg0)
      clearUpon: 116
      callSubroutine: s32'SpecialAttackC_Init_NRM'
    endIf: 
  endIf: 
  else: 
    callSubroutine: s32'cmnSpecialAtkLv3_AtkInit'
    callSubroutine: s32'cmnSpecialAttack_Init'
    if: Mem(ActReg0)
      clearUpon: 116
    endIf: 
  endElse: 
  groundHitEffect: (AIR_FACE_UP)
  airHitEffect: (AIR_FACE_UP)
  hitAirPushbackX: 8000
  hitAirPushbackY: 10000
  untechAmount: 60
  clashLevel: 2
