beginSubroutine: s32'Assault_Genji_CancelInit'
    storeValue: Mem(ActReg0), Mem(ObjReg1)
    checkLastStateName: s32'Assault_Genji_A'
    if: Mem(Tmp)
        storeValue: Mem(ActReg0), Val(1)
        isObjectActive: (STACK0)
        ifNot: Mem(Tmp)
            exPointFReset: (CENTER), 0, 50000
            createParticle: s32'nrm_kaguraIcon', (EX_POINT_F)
            privateSE: s32'nrm_15'
        endIf: 
    endIf: 
    else: 
        checkLastStateName: s32'Assault_Genji_B'
        if: Mem(Tmp)
            storeValue: Mem(ActReg0), Val(1)
            isObjectActive: (STACK0)
            ifNot: Mem(Tmp)
                exPointFReset: (CENTER), 0, 50000
                createParticle: s32'nrm_kaguraIcon', (EX_POINT_F)
                privateSE: s32'nrm_15'
            endIf: 
        endIf: 
        else: 
            checkLastStateName: s32'Assault_Genji_C'
            if: Mem(Tmp)
                storeValue: Mem(ActReg0), Val(1)
                isObjectActive: (STACK0)
                ifNot: Mem(Tmp)
                    exPointFReset: (CENTER), 0, 50000
                    createParticle: s32'nrm_kaguraIcon', (EX_POINT_F)
                    privateSE: s32'nrm_15'
                endIf: 
            endIf: 
        endElse: 
    endElse: 
    if: Mem(ActReg0)
        storeValue: Mem(PlayerVal3), Val(0)
        clearUpon: 116
    endIf: 
endSubroutine: 

