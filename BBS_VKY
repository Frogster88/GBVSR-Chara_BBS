beginSubroutine: s32'OnFrameStep'
  callSubroutine: s32'cmnTest'
  ifOperation: (IS_EQUAL), Mem(PlayerVal2), Val(0)
    Unknown1925: 1, 1
  endIf: 
  ifOperation: (IS_EQUAL), Mem(PlayerVal2), Val(1)
    Unknown1925: 1, 0
  endIf: 
  ifOperation: (IS_EQUAL), Mem(PlayerVal2), Val(10)
    Unknown1925: 1, 1
  endIf: 
  ifOperation: (IS_EQUAL), Mem(PlayerVal2), Val(11)
    Unknown1925: 1, 0
  endIf: 
endSubroutine: 

beginSubroutine: s32'OnActionBegin'
  checkSomething: 3, 3
  if: Mem(Tmp)
    Unknown1924: 0, 1
    Unknown1924: 1, 1
    Unknown1924: 2, 1
    Unknown1924: 3, 1
  endIf: 
  else: 
    Unknown1924: 0, 0
    Unknown1924: 1, 0
    Unknown1924: 2, 0
    Unknown1924: 3, 0
  endElse: 
endSubroutine: 

beginSubroutine: s32'OnFinalize'
  callSubroutine: s32'cmn_FN_OnFinalize'
endSubroutine: 

beginSubroutine: s32'OnIdling'
  ifOperation: (IS_EQUAL), Mem(ActionTime), Val(3)
    callSubroutine: s32'AbilityCoolDown_N'
    callSubroutine: s32'AbilityCoolDown_6'
    callSubroutine: s32'AbilityCoolDown_6_ON_FINALIZE'
    callSubroutine: s32'AbilityCoolDown_4'
    callSubroutine: s32'AbilityCoolDown_2'
  endIf: 
  ifNot: Mem(BattleDoing)
    ifOperation: (IS_EQUAL), Mem(HitPoint), Val(0)
      modifyVar: (ADD), Mem(276), Val(1)
    endIf: 
    else: 
      Unknown310: 9, 22, 2, 77, 0, 0
      modifyVar: (ADD), Mem(276), Val(1)
    endIf: 
    else: 
      if: Mem(381)
        modifyVar: (ADD), Mem(276), Val(1)
      endIf: 
    endElse: 
  endElse: 
  ifOperation: (IS_EQUAL), Mem(276), Val(60)
    pawnAnimation: 4, s32'NotBattleDoing', 0
    pawnAnimation: 5, s32'NotBattleDoing', 0
    pawnAnimation: 6, s32'NotBattleDoing', 0
    pawnAnimation: 7, s32'NotBattleDoing', 0
    pawnAnimation: 8, s32'NotBattleDoing', 0
  endIf: 
endIf: 

endSubroutine: 
beginSubroutine: s32'OnDamage'
  callSubroutine: s32'cmn_Fn_OnDamage'
  ifOperation: (IS_LESSER_OR_EQUAL), Mem(ActionTime), Val(2)
    callSubroutine: s32'AbilityCoolDown_N'
    callSubroutine: s32'AbilityCoolDown_6_DAMAGE'
    callSubroutine: s32'AbilityCoolDown_4'
    callSubroutine: s32'AbilityCoolDown_2'
  endIf: 
endSubroutine: 

beginSubroutine: s32'AbilityCoolDown_6_DAMAGE'
  checkCurrentStateName: (SELF), s32'Firework_A'
  if: Mem(Tmp)
    storeValue: Mem(288), Val(20)
    Unknown1923: 1
    Unknown1681: 
  endIf: 
  checkCurrentStateName: (SELF), s32'Firework_B'
  if: Mem(Tmp)
    storeValue: Mem(288), Val(20)
    Unknown1923: 1
    Unknown1681: 
  endIf: 
  checkCurrentStateName: (SELF), s32'Firework_C'
  if: Mem(Tmp)
    storeValue: Mem(288), Val(420)
    Unknown1923: 1
  endIf: 
  checkCurrentStateName: (SELF), s32'Firework_D'
  if: Mem(Tmp)
    if: Mem(288)
      storeValue: Mem(417), Val(1)
    endIf: 
    modifyVar2: (ADD), Val(300), Mem(288)
    storeValue: Mem(288), Mem(Tmp)
    Unknown1923: 1
  endIf: 
endSubroutine: 

beginSubroutine: s32'AbilityCoolDown_6_ON_FINALIZE'
  checkCurrentStateName: (SELF), s32'Firework_A'
  if: Mem(Tmp)
    upon: (BEFORE_EXIT)
      storeValue: Mem(288), Val(20)
      Unknown1923: 1
      storeValue: Mem(PlayerVal2), Val(0)
    endUpon: 
  endIf: 
  checkCurrentStateName: (SELF), s32'Firework_B'
  if: Mem(Tmp)
    upon: (BEFORE_EXIT)
      storeValue: Mem(288), Val(20)
      Unknown1923: 1
      storeValue: Mem(PlayerVal2), Val(0)
    endUpon: 
  endIf: 
  checkCurrentStateName: (SELF), s32'Firework_C'
  if: Mem(Tmp)
    upon: (BEFORE_EXIT)
      storeValue: Mem(288), Val(420)
      Unknown1923: 1
      storeValue: Mem(PlayerVal2), Val(0)
    endUpon: 
  endIf: 
  checkCurrentStateName: (SELF), s32'Firework_D'
  if: Mem(Tmp)
    upon: (BEFORE_EXIT)
      storeValue: Mem(PlayerVal2), Val(0)
    endUpon: 
  endIf: 
endSubroutine: 

