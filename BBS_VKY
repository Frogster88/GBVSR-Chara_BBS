beginSubroutine: s32'Func_Cannon_Add_Item'
  ifOperation: (IS_LESSER_OR_EQUAL), Mem(ObjReg5), Val(40)
    storeValue: Mem(ActReg7), Val(1)
  endIf: 
  else: 
    ifOperation: (IS_LESSER_OR_EQUAL), Mem(ObjReg5), Val(45)
      storeValue: Mem(ActReg7), Val(2)
    endIf: 
    else: 
      ifOperation: (IS_LESSER_OR_EQUAL), Mem(ObjReg5), Val(65)
        storeValue: Mem(ActReg7), Val(3)
      endIf: 
      else: 
        ifOperation: (IS_LESSER_OR_EQUAL), Mem(ObjReg5), Val(75)
          storeValue: Mem(ActReg7), Val(4)
        endIf: 
        else: 
          ifOperation: (IS_LESSER_OR_EQUAL), Mem(ObjReg5), Val(80)
            storeValue: Mem(ActReg7), Val(5)
          endIf: 
          else: 
            ifOperation: (IS_LESSER_OR_EQUAL), Mem(ObjReg5), Val(90)
              storeValue: Mem(ActReg7), Val(6)
            endIf: 
            else: 
              ifOperation: (IS_LESSER_OR_EQUAL), Mem(ObjReg5), Val(95)
                storeValue: Mem(ActReg7), Val(7)
              endIf: 
              else: 
                ifOperation: (IS_LESSER_OR_EQUAL), Mem(ObjReg5), Val(100)
                  storeValue: Mem(ActReg7), Val(8)
                endIf: 
                else: 
                endElse: 
              endElse: 
            endElse: 
          endElse: 
        endElse: 
      endElse: 
    endElse: 
  endElse: 
  if: Mem(IsTrainingMode)
    getTrainingItemVal: s32'ETMI_UniqueGauge_VKY_Choice', 2, 61
    if: Mem(GTmpReg0)
      storeValue: Mem(ActReg7), Mem(GTmpReg0)
    endIf: 
  endIf: 
  ifOperation: (IS_EQUAL), Mem(PlayerVal1), Val(2)
    storeValue: Mem(ActReg7), Val(3)
  endIf: 
  ifOperation: (IS_EQUAL), Mem(PlayerVal1), Val(7)
    storeValue: Mem(ActReg7), Val(2)
  endIf: 
endSubroutine: 

