  endIf: 
  ifOperation: (IS_EQUAL), Mem(ActReg7), Val(9)
    createObject: s32'Item09_ShotAtk', (PLAYER)
  endIf: 
  stackObject: (STACK0), (PREVIOUS)
endSubroutine: 

