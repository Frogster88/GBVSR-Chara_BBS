  sprite: s32'vky207_12', 2
endState: 

beginState: s32'NmlAtk2C'
  upon: (IMMEDIATE)
    callSubroutine: s32'cmnAtk2C_AtkInit'
    addGatlingOption: s32'NmlAtk5D'
    addGatlingOption: s32'NmlAtk6D'
