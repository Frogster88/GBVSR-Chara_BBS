beginSubroutine: s32'Func_Atk5DInit'
  attackIgnoreCaution: 1
  enableSpecialCancel: 0
  Unknown2197: 0x020000003A00000000000000010000000000000064000000
  ifOperation: (IS_LESSER_OR_EQUAL), Mem(ObjReg5), Val(40)
    storeValue: Mem(ActReg7), Val(1)
  endIf: 
  else: 
    ifOperation: (IS_LESSER_OR_EQUAL), Mem(ObjReg5), Val(45)
      storeValue: Mem(ActReg7), Val(2)
    endIf: 
    else: 
      ifOperation: (IS_LESSER_OR_EQUAL), Mem(ObjReg5), Val(65)
        storeValue: Mem(ActReg7), Val(3)
      endIf: 
      else: 
        ifOperation: (IS_LESSER_OR_EQUAL), Mem(ObjReg5), Val(75)
          storeValue: Mem(ActReg7), Val(4)
        endIf: 
        else: 
          ifOperation: (IS_LESSER_OR_EQUAL), Mem(ObjReg5), Val(80)
            storeValue: Mem(ActReg7), Val(5)
          endIf: 
          else: 
            ifOperation: (IS_LESSER_OR_EQUAL), Mem(ObjReg5), Val(90)
              storeValue: Mem(ActReg7), Val(6)
            endIf: 
            else: 
              ifOperation: (IS_LESSER_OR_EQUAL), Mem(ObjReg5), Val(95)
                storeValue: Mem(ActReg7), Val(7)
              endIf: 
              else: 
                ifOperation: (IS_LESSER_OR_EQUAL), Mem(ObjReg5), Val(100)
                  storeValue: Mem(ActReg7), Val(8)
                endIf: 
                else: 
                endElse: 
              endElse: 
            endElse: 
          endElse: 
        endElse: 
      endElse: 
    endElse: 
  endElse: 
  upon: (FRAME_STEP)
    ifOperation: (IS_GREATER_OR_EQUAL), Mem(ActionTime), Val(104)
      gotoLabelRequests: s32'End'
    endIf: 
    if: Mem(IsAir)
      ifOperation: (IS_GREATER_OR_EQUAL), Mem(ActionTime), Val(3)
        ifOperation: (IS_LESSER_OR_EQUAL), Mem(PosY), Val(10000)
          haltMomentum: 1
        endIf: 
      endIf: 
    endIf: 
    callSubroutine: s32'Func_ItemInit_Random'
  endUpon: 
  upon: 36
    ifOperation: (IS_EQUAL), Mem(ActionTime), Val(41)
      clearUpon: 36
      attackEndDelay: 
    endIf: 
  endUpon: 
endSubroutine: 

beginSubroutine: s32'Func_Atk5D_ButtonInit'
  if: Mem(ActReg1)
    if: Mem(BattleDoing)
      checkInput: 23
      if: Mem(Tmp)
        gotoLabelRequests: s32'Shot'
      endIf: 
      ifOperation: (IS_GREATER_OR_EQUAL), Mem(ActReg1), Val(2)
        checkInput: (INPUT_HOLD_L)
        if: Mem(Tmp)
          gotoLabelRequests: s32'End'
        endIf: 
        checkInput: (INPUT_HOLD_M)
        if: Mem(Tmp)
          gotoLabelRequests: s32'End'
        endIf: 
        checkInput: (INPUT_HOLD_H)
        if: Mem(Tmp)
          gotoLabelRequests: s32'End'
        endIf: 
        checkInput: (INPUT_HOLD_G)
        if: Mem(Tmp)
          gotoLabelRequests: s32'End'
        endIf: 
        checkInput: 127
        if: Mem(Tmp)
          gotoLabelRequests: s32'End'
        endIf: 
      endIf: 
    endIf: 
    else: 
      gotoLabelRequests: s32'End'
    endElse: 
  endIf: 
endSubroutine: 

beginSubroutine: s32'Func_ItemInit_Random'
  if: Mem(ActReg0)
    modifyVar: (ADD), Mem(ActReg0), Val(1)
    ifOperation: (MOD_EQUALS_2), Mem(ActReg0), Val(8)
      storeValue: Mem(ActReg0), Val(0)
      if: Mem(IsTrainingMode)
        getTrainingItemVal: s32'ETMI_UniqueGauge_VKY_Choice', 2, 61
        if: Mem(GTmpReg0)
          storeValue: Mem(ActReg7), Mem(GTmpReg0)
        endIf: 
      endIf: 
      ifOperation: (IS_EQUAL), Mem(PlayerVal1), Val(2)
        storeValue: Mem(ActReg7), Val(3)
      endIf: 
      ifOperation: (IS_EQUAL), Mem(PlayerVal1), Val(7)
        storeValue: Mem(ActReg7), Val(2)
      endIf: 
      ifOperation: (IS_EQUAL), Mem(ActReg7), Val(1)
        pawnAnimation: 4, s32'Item01', 0
      endIf: 
      ifOperation: (IS_EQUAL), Mem(ActReg7), Val(2)
        pawnAnimation: 4, s32'Item02', 0
      endIf: 
      ifOperation: (IS_EQUAL), Mem(ActReg7), Val(3)
        pawnAnimation: 4, s32'Item03', 0
      endIf: 
      ifOperation: (IS_EQUAL), Mem(ActReg7), Val(4)
        pawnAnimation: 4, s32'Item04', 0
      endIf: 
      ifOperation: (IS_EQUAL), Mem(ActReg7), Val(5)
        pawnAnimation: 4, s32'Item05', 0
      endIf: 
      ifOperation: (IS_EQUAL), Mem(ActReg7), Val(6)
        pawnAnimation: 4, s32'Item06', 0
      endIf: 
      ifOperation: (IS_EQUAL), Mem(ActReg7), Val(7)
        pawnAnimation: 4, s32'Item07', 0
      endIf: 
      ifOperation: (IS_EQUAL), Mem(ActReg7), Val(8)
        pawnAnimation: 4, s32'Item08', 0
      endIf: 
      ifOperation: (IS_EQUAL), Mem(ActReg7), Val(9)
        pawnAnimation: 4, s32'Item09', 0
      endIf: 
    endIf: 
  endIf: 
  callSubroutine: s32'Func_Atk5D_ButtonInit'
endSubroutine: 

