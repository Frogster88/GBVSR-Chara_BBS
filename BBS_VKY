beginSubroutine: s32'AbilityCoolDown_6'
  checkCurrentStateName: (SELF), s32'Firework_A'
  if: Mem(Tmp)
    skillCooldown: 1, 20
    Unknown1923: 1
    Unknown1681: 
    modifyVar: (ADD), Mem(PlayerVal2), Val(1)
  endIf: 
  checkCurrentStateName: (SELF), s32'Firework_B'
  if: Mem(Tmp)
    skillCooldown: 1, 20
    Unknown1923: 1
    Unknown1681: 
    modifyVar: (ADD), Mem(PlayerVal2), Val(1)
  endIf: 
  checkCurrentStateName: (SELF), s32'Firework_C'
  if: Mem(Tmp)
    skillCooldown: 1, 420
    Unknown1923: 1
    modifyVar: (ADD), Mem(PlayerVal2), Val(1)
  endIf: 
  checkCurrentStateName: (SELF), s32'Firework_D'
  if: Mem(Tmp)
