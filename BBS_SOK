beginState: s32'SpecialJump_A'
    upon: (IMMEDIATE)
        callSubroutine: s32'SpecialJump_1st'
        callSubroutine: s32'bullet_check'
        upon: (FRAME_STEP)
            ifOperation: (IS_EQUAL), Mem(ActionTime), Val(4)
                ifNot: Mem(ActReg6)
                    storeValue: Mem(ActReg0), Val(0)
                    storeValue: Mem(ActReg1), Val(1)
                endIf: 
            endIf: 
            ifOperation: (IS_GREATER_OR_EQUAL), Mem(ActionTime), Val(4)
                ifNot: Mem(ActReg1)
                    checkInput: (INPUT_HOLD_L)
                    if: Mem(Tmp)
                        storeValue: Mem(ActReg0), Val(1)
                    endIf: 
                    else: 
                        checkInput: 25
                        if: Mem(Tmp)
                            storeValue: Mem(ActReg0), Val(1)
                        endIf: 
                        else: 
                            storeValue: Mem(ActReg0), Val(0)
                            storeValue: Mem(ActReg1), Val(1)
                        endElse: 
                    endElse: 
                endIf: 
            endIf: 
            ifOperation: (IS_EQUAL), Mem(ActionTime), Val(17)
