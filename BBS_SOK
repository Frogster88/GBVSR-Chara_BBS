beginSubroutine: s32'bullet_check'
    upon: 12
        ifOperation: (IS_GREATER_OR_EQUAL), Mem(ActionTime), Val(3)
            clearUpon: 12
            checkSomething: 3, 1
            if: Mem(Tmp)
                storeValue: Mem(ActReg6), Val(1)
            endIf: 
            else: 
                if: Mem(ExGage0)
                    modifyVar: (ADD), Mem(ExGage0), Val(-1)
                    storeValue: Mem(ActReg6), Val(1)
                endIf: 
            endElse: 
        endIf: 
    endUpon: 
endSubroutine: 

