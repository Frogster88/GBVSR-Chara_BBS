beginSubroutine: s32'OnDamage'
    callSubroutine: s32'cmn_Fn_OnDamage'
    ifNot: Mem(363)
        ifOperation: (IS_EQUAL), Mem(403), Val(0)
            pawnAnimation: 4, s32'lwa050', 0
            pawnAnimation: 5, s32'lwb050', 0
            pawnAnimation: 6, s32'lwc050', 0
        endIf: 
        ifOperation: (IS_EQUAL), Mem(403), Val(1)
            pawnAnimation: 4, s32'lwa052', 0
            pawnAnimation: 5, s32'lwb052', 0
            pawnAnimation: 6, s32'lwc052', 0
        endIf: 
    endIf: 
    if: Mem(PlayerVal0)
        storeValue: Mem(PlayerVal0), Val(0)
    endIf: 
    ifOperation: (IS_LESSER_OR_EQUAL), Mem(ActionTime), Val(2)
        callSubroutine: s32'AbilityCoolDown_N'
        callSubroutine: s32'AbilityCoolDown_6'
        callSubroutine: s32'AbilityCoolDown_4'
        callSubroutine: s32'AbilityCoolDown_2'
    endIf: 
endSubroutine: 

beginSubroutine: s32'OnIdling'
    if: Mem(PlayerVal0)
        addMoveForceDisableFlag: 268435456
        addMoveForceDisableFlag: 536870912
        addMoveForceDisableFlag: 1073741824
        enableJump: 0
        enableForwardDash: 0
        enableBackDash: 0
        disableCrouch: 1
        Unknown2361: 1
        Unknown1925: 0, 2
        Unknown1925: 1, 2
        Unknown1925: 2, 2
        Unknown1925: 3, 2
    endIf: 
    else: 
        disableCrouch: 0
        Unknown2361: 0
        Unknown1925: 0, 1
        Unknown1925: 1, 1
        Unknown1925: 2, 1
        Unknown1925: 3, 1
    endElse: 
    ifOperation: (IS_EQUAL), Mem(ActionTime), Val(3)
        callSubroutine: s32'AbilityCoolDown_N'
        callSubroutine: s32'AbilityCoolDown_6'
        callSubroutine: s32'AbilityCoolDown_4'
        callSubroutine: s32'AbilityCoolDown_2'
    endIf: 
endSubroutine: 

beginSubroutine: s32'KAKUJIN_Init'
    modifyVar: (ADD), Mem(ExGage1), Val(2500)
    callSubroutine: s32'KAKUJIN_LV_Setting'
    ifOperation: (IS_GREATER_OR_EQUAL), Mem(ExGage0), Val(2)
        counterHitSoftKnockdown: 0, 1100
    endIf: 
    ifOperation: (IS_GREATER_OR_EQUAL), Mem(ExGage0), Val(4)
        counterHitSoftKnockdown: 0, 1200
    endIf: 
    ifOperation: (IS_GREATER_OR_EQUAL), Mem(ExGage0), Val(5)
        counterHitSoftKnockdown: 0, 1300
    endIf: 
endSubroutine: 

