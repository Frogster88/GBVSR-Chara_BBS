beginSubroutine: s32'PreInit2nd'
    storeValue: Mem(GTmpReg0), Val(0)
    ifNot: Mem(GTmpReg0)
        ifOperation: (IS_EQUAL), Mem(SideID), Val(0)
            entryAction: s32'entry_default', s32'', 0
            storeValue: Mem(GTmpReg0), Val(1)
        endIf: 
        else: 
            entryAction: s32'entry_2p01', s32'entry_1p01', 3
            storeValue: Mem(GTmpReg0), Val(1)
        endElse: 
    endIf: 
endSubroutine: 

