beginSubroutine: s32'KAKUJIN_LV_Setting'
  ifOperation: (IS_LESSER_OR_EQUAL), Mem(ExGage1), Val(9999)
    storeValue: Mem(ExGage0), Val(0)
    Unknown1889: 1, 0
  endIf: 
  else: 
    ifOperation: (IS_LESSER_OR_EQUAL), Mem(ExGage1), Val(19999)
      storeValue: Mem(ExGage0), Val(1)
      Unknown1889: 1, 1
    endIf: 
    else: 
      ifOperation: (IS_LESSER_OR_EQUAL), Mem(ExGage1), Val(29999)
        storeValue: Mem(ExGage0), Val(2)
        Unknown1889: 1, 2
      endIf: 
      else: 
        ifOperation: (IS_LESSER_OR_EQUAL), Mem(ExGage1), Val(39999)
          storeValue: Mem(ExGage0), Val(3)
          Unknown1889: 1, 3
        endIf: 
        else: 
          ifOperation: (IS_LESSER_OR_EQUAL), Mem(ExGage1), Val(49999)
            storeValue: Mem(ExGage0), Val(4)
            Unknown1889: 1, 4
          endIf: 
          else: 
            storeValue: Mem(ExGage0), Val(5)
            Unknown1889: 1, 5
          endElse: 
        endElse: 
      endElse: 
    endElse: 
  endElse: 
endSubroutine: 

beginSubroutine: s32'OnFrameStep'
  callSubroutine: s32'cmnTest'
  callSubroutine: s32'KAKUJIN_LV_Setting'
  ifNot: Mem(BattleDoing)
    checkCurrentStateName: (SELF), s32'CmnActStand'
    if: Mem(Tmp)
      if: Mem(393)
        callSubroutine: s32'Weapon_Disp'
      endIf: 
      else: 
        callSubroutine: s32'Weapon_NoDisp'
      endElse: 
    endIf: 
  endIf: 
  ifNot: Mem(295)
    ifNot: Mem(DebugSkillEx)
      if: Mem(BattleDoing)
        ifOperation: (IS_LESSER), Mem(ObjReg5), Mem(ExGage0)
          modifyVar: (ADD), Mem(ObjReg5), Val(1)
          deactivateObjectByName: s32'Kakujin_lvupEff'
          createObject: s32'Kakujin_lvupEff', (PLAYER)
        endIf: 
        ifOperation: (IS_EQUAL), Mem(ExGage0), Val(5)
          modifyVar: (ADD), Mem(ObjReg5), Val(1)
          ifOperation: (MOD_EQUALS_0), Mem(ObjReg5), Val(12)
            createParticle: s32'luc_LvMaxAura', (HEAD_3D)
            createParticle: s32'luc_LvMaxAura', (ELBOW_L_3D)
            createParticle: s32'luc_LvMaxAura', (ELBOW_R_3D)
            createParticle: s32'luc_LvMaxAura', (CALF_L_3D)
            createParticle: s32'luc_LvMaxAura', (CALF_R_3D)
            createParticle: s32'luc_LvMaxAura', (FOOT_L_3D)
            createParticle: s32'luc_LvMaxAura', (FOOT_R_3D)
          endIf: 
        endIf: 
      endIf: 
    endIf: 
  endIf: 
  if: Mem(IsTrainingMode)
    if: Mem(BattleDoing)
      if: Mem(IsRecoverableTypeA)
        getTrainingItemVal: s32'ETMI_UniqueGauge_LUC_BladeLevel', 2, 61
        ifOperation: (IS_GREATER_OR_EQUAL), Mem(GTmpReg0), Val(1)
          storeValue: Mem(ExGage1), Val(10000)
        endIf: 
        ifOperation: (IS_GREATER_OR_EQUAL), Mem(GTmpReg0), Val(2)
          storeValue: Mem(ExGage1), Val(20000)
        endIf: 
        ifOperation: (IS_GREATER_OR_EQUAL), Mem(GTmpReg0), Val(3)
          storeValue: Mem(ExGage1), Val(30000)
        endIf: 
        ifOperation: (IS_GREATER_OR_EQUAL), Mem(GTmpReg0), Val(4)
          storeValue: Mem(ExGage1), Val(40000)
        endIf: 
        ifOperation: (IS_GREATER_OR_EQUAL), Mem(GTmpReg0), Val(5)
          storeValue: Mem(ExGage1), Val(50000)
        endIf: 
      endIf: 
    endIf: 
  endIf: 
  ifOperation: (IS_EQUAL), Mem(ExGage0), Val(1)
    Unknown2347: 1600353612, 0x4C763100000000000000000000000000000000000000000000000000
  endIf: 
  ifOperation: (IS_EQUAL), Mem(ExGage0), Val(2)
    Unknown2347: 1600353612, 0x4C763200000000000000000000000000000000000000000000000000
  endIf: 
  ifOperation: (IS_EQUAL), Mem(ExGage0), Val(3)
    Unknown2347: 1600353612, 0x4C763300000000000000000000000000000000000000000000000000
  endIf: 
  ifOperation: (IS_EQUAL), Mem(ExGage0), Val(4)
    Unknown2347: 1600353612, 0x4C763400000000000000000000000000000000000000000000000000
  endIf: 
  ifOperation: (IS_EQUAL), Mem(ExGage0), Val(5)
