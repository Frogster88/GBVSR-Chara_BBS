beginSubroutine: s32'Ougi2_Hasei'
    if: Mem(ActReg1)
        ifNot: Mem(ActReg5)
            checkInput: (INPUT_PRESS_L)
            if: Mem(Tmp)
                storeValue: Mem(ActReg5), Val(1)
            endIf: 
            else: 
                checkInput: (INPUT_PRESS_M)
                if: Mem(Tmp)
                    storeValue: Mem(ActReg5), Val(2)
                endIf: 
                else: 
                    checkInput: (INPUT_PRESS_H)
                    if: Mem(Tmp)
                        storeValue: Mem(ActReg5), Val(3)
                    endIf: 
                    else: 
                        checkInput: (INPUT_PRESS_U)
                        if: Mem(Tmp)
                            storeValue: Mem(ActReg5), Val(4)
                        endIf: 
                        else: 
                            checkInput: (INPUT_PRESS_S)
                            if: Mem(Tmp)
                                storeValue: Mem(ActReg5), Val(3)
                            endIf: 
                        endElse: 
                    endElse: 
                endElse: 
            endElse: 
        endIf: 
    endIf: 
    else: 
        if: Mem(ActReg5)
            ifOperation: (IS_EQUAL), Mem(ActReg5), Val(1)
                callSubroutine: s32'Ougi2_ClearSetUp'
                gotoLabelRequests: s32'Low'
            endIf: 
            ifOperation: (IS_EQUAL), Mem(ActReg5), Val(2)
                callSubroutine: s32'Ougi2_ClearSetUp'
                gotoLabelRequests: s32'High'
            endIf: 
            ifOperation: (IS_EQUAL), Mem(ActReg5), Val(3)
                callSubroutine: s32'Ougi2_ClearSetUp'
                gotoLabelRequests: s32'Middle'
            endIf: 
            ifOperation: (IS_EQUAL), Mem(ActReg5), Val(4)
                clearUpon: (FRAME_STEP)
                gotoLabelRequests: s32'Apocalypse_Hasei'
            endIf: 
        endIf: 
        else: 
            checkInput: (INPUT_HOLD_L)
            if: Mem(Tmp)
                callSubroutine: s32'Ougi2_ClearSetUp'
                gotoLabelRequests: s32'Low'
            endIf: 
            else: 
                checkInput: (INPUT_HOLD_M)
                if: Mem(Tmp)
                    callSubroutine: s32'Ougi2_ClearSetUp'
                    gotoLabelRequests: s32'High'
                endIf: 
                else: 
                    checkInput: (INPUT_HOLD_H)
                    if: Mem(Tmp)
                        callSubroutine: s32'Ougi2_ClearSetUp'
                        gotoLabelRequests: s32'Middle'
                    endIf: 
                    else: 
                        checkInput: (INPUT_HOLD_U)
                        if: Mem(Tmp)
                            clearUpon: (FRAME_STEP)
                            gotoLabelRequests: s32'Apocalypse_Hasei'
                        endIf: 
                        else: 
                            checkInput: 25
                            if: Mem(Tmp)
                                callSubroutine: s32'Ougi2_ClearSetUp'
                                gotoLabelRequests: s32'Middle'
                            endIf: 
                        endElse: 
                    endElse: 
                endElse: 
            endElse: 
            modifyVar: (ADD), Mem(ActReg2), Val(1)
            ifOperation: (IS_GREATER_OR_EQUAL), Mem(ActReg2), Val(60)
                gotoLabelRequests: s32'End'
            endIf: 
            ifNot: Mem(BattleDoing)
                gotoLabelRequests: s32'End'
            endIf: 
        endElse: 
    endElse: 
endSubroutine: 

