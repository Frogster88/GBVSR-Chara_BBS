            callSubroutine: s32'luc_AbilityCoolDown0_Set'
        endUpon: 
    endIf: 
    checkCurrentStateName: (SELF), s32'SpecialShot_B'
    if: Mem(Tmp)
        upon: (BEFORE_EXIT)
            callSubroutine: s32'luc_AbilityCoolDown0_Set'
        endUpon: 
