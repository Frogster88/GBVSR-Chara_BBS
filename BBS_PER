beginSubroutine: s32'BuffLimit'
  ifOperation: (IS_EQUAL), Mem(ObjReg4), Val(5)
    ifOperation: (IS_GREATER_OR_EQUAL), Mem(ExGage0), Val(6)
      storeValue: Mem(ExGage0), Val(5)
    endIf: 
  endIf: 
  ifOperation: (IS_EQUAL), Mem(ObjReg4), Val(6)
    ifOperation: (IS_GREATER_OR_EQUAL), Mem(ExGage0), Val(7)
      storeValue: Mem(ExGage0), Val(6)
    endIf: 
  endIf: 
  ifOperation: (IS_EQUAL), Mem(ObjReg4), Val(7)
    ifOperation: (IS_GREATER_OR_EQUAL), Mem(ExGage0), Val(8)
      storeValue: Mem(ExGage0), Val(7)
    endIf: 
  endIf: 
  ifOperation: (IS_EQUAL), Mem(ObjReg4), Val(8)
    ifOperation: (IS_GREATER_OR_EQUAL), Mem(ExGage0), Val(9)
      storeValue: Mem(ExGage0), Val(8)
    endIf: 
  endIf: 
  ifOperation: (IS_GREATER_OR_EQUAL), Mem(ObjReg4), Val(9)
    ifOperation: (IS_GREATER_OR_EQUAL), Mem(ExGage0), Val(10)
      storeValue: Mem(ExGage0), Val(9)
    endIf: 
  endIf: 
endSubroutine: 

