beginSubroutine: s32'OnFrameStep'
  callSubroutine: s32'cmnTest'
  if: Mem(IsTrainingMode)
    if: Mem(IsRecoverableTypeA)
      getTrainingItemVal: s32'ETMI_UniGauge_PER_Traeumerei', 2, 61
      ifOperation: (IS_LESSER_OR_EQUAL), Mem(ExGage0), Mem(GTmpReg0)
        storeValue: Mem(ExGage0), Mem(GTmpReg0)
        ifOperation: (IS_GREATER_OR_EQUAL), Mem(GTmpReg0), Val(5)
          storeValue: Mem(ObjReg4), Mem(GTmpReg0)
        endIf: 
      endIf: 
    endIf: 
  endIf: 
  ifNot: Mem(Timeup)
    if: Mem(ExGage0)
      ifNot: Mem(ObjReg0)
        storeValue: Mem(ObjReg0), Val(1)
        Unknown412: 100, 1, 6, 1
      endIf: 
    endIf: 
    else: 
      if: Mem(ObjReg0)
        storeValue: Mem(ObjReg0), Val(0)
        Unknown412: 0, 1, 6, 1
      endIf: 
    endElse: 
    if: Mem(ObjReg1)
      ifOperation: (IS_EQUAL), Mem(ObjReg1), Val(2)
        storeValue: Mem(ObjReg1), Val(1)
      endIf: 
      else: 
        ifOperation: (IS_EQUAL), Mem(ObjReg1), Val(1)
          storeValue: Mem(ObjReg1), Val(0)
          Unknown412: 100, 1, 6, 1
        endIf: 
      endElse: 
    endIf: 
    if: Mem(ObjReg3)
      storeValue: Mem(ObjReg3), Val(0)
    endIf: 
  endIf: 
  else: 
    ifNot: Mem(ObjReg3)
      Unknown412: 0, 1, 6, 1
      storeValue: Mem(ObjReg0), Val(0)
      storeValue: Mem(ObjReg3), Val(1)
    endIf: 
  endElse: 
  modifyVar: (ADD), Mem(ExGage1), Val(-1)
  if: Mem(PlayerDamage)
    checkCurrentStateName: (SELF), s32'CmnActHajikareStand'
    ifNot: Mem(Tmp)
      checkCurrentStateName: (SELF), s32'CmnActHajikareCrouch'
      ifNot: Mem(Tmp)
        checkCurrentStateName: (SELF), s32'CmnActHajikareAir'
        ifNot: Mem(Tmp)
          checkCurrentStateName: (SELF), s32'CmnActLockWait'
          ifNot: Mem(Tmp)
            checkCurrentStateName: (SELF), s32'CmnActAirLockReject'
            ifNot: Mem(Tmp)
              sendSignalToAction: s32'per_401_SpecialShot_D_TM', 23
            endIf: 
          endIf: 
        endIf: 
      endIf: 
    endIf: 
  endIf: 
endSubroutine: 

