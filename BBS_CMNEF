  throwRejectType: 0
  Unknown1168: 1, 1
  attackType: 1
  attackRange: 300000, -50000, 0, 0
  throwRange: 105000
  onlyHitGrounded: 1
  doubleSlashEffectBugFix: 1
  tensionGainOnConnect: 40
  checkCurrentStateName: (SELF), s32'NmlAtkThrow'
  if: Mem(Tmp)
    attackLockAction: s32'ThrowExe'
  endIf: 
  checkCurrentStateName: (SELF), s32'NmlAtkBackThrow'
  if: Mem(Tmp)
    attackLockAction: s32'BackThrowExe'
  endIf: 
  haltMomentum: 1
  if: Mem(386)
    upon: 116
      ifOperation: (IS_EQUAL), Mem(ActionTime), Val(15)
        missionMissName: s32'TrialMiss_Throw'
      endIf: 
    endUpon: 
  endIf: 
endSubroutine: 

