beginSubroutine: s32'cmnActLockWaitTime'
  upon: 116
    checkInput: (INPUT_PRESS_L)
    if: Mem(Tmp)
      storeValue: Mem(ObjReg7), Val(1)
      checkInput: (INPUT_PRESS_M)
      if: Mem(Tmp)
        storeValue: Mem(ObjReg7), Val(0)
      endIf: 
      checkInput: (INPUT_PRESS_U)
      if: Mem(Tmp)
        storeValue: Mem(ObjReg7), Val(0)
      endIf: 
    endIf: 
    checkInput: (INPUT_PRESS_M)
    if: Mem(Tmp)
      storeValue: Mem(ObjReg7), Val(1)
      checkInput: (INPUT_PRESS_L)
      if: Mem(Tmp)
        storeValue: Mem(ObjReg7), Val(0)
      endIf: 
    endIf: 
    checkInput: (INPUT_PRESS_H)
    if: Mem(Tmp)
      storeValue: Mem(ObjReg7), Val(1)
    endIf: 
    checkInput: (INPUT_PRESS_U)
    if: Mem(Tmp)
      storeValue: Mem(ObjReg7), Val(1)
      checkInput: (INPUT_PRESS_L)
      if: Mem(Tmp)
        storeValue: Mem(ObjReg7), Val(0)
      endIf: 
    endIf: 
    checkInput: (INPUT_THROW)
    if: Mem(Tmp)
      storeValue: Mem(ObjReg7), Val(0)
    endIf: 
  endUpon: 
endSubroutine: 

