beginState: s32'SwordfLumiere_D'
  upon: (IMMEDIATE)
    callSubroutine: s32'SwordfLumiere_Init'
    callSubroutine: s32'cmnSpecialAttackD_Init'
    damageEffect: 2, s32'CHA_407_HitEff_D'
    hitAirPushbackX: -10000
    hitAirPushbackY: 8000
    clashLevel: 4
    hitstop: 3
    enemyHitstopAddition: 10, 0, 0
    wallPushback: 0, 100
    groundHitEffect: (AIR_FACE_UP)
    airHitEffect: (AIR_FACE_UP)
    attackRange: 1000000, 50000, 1000000, -1
    storeValue: Mem(ObjReg0), Val(0)
    storeValue: Mem(ObjReg2), Val(0)
    if: Mem(290)
      storeValue: Mem(ObjReg2), Val(1)
      callSubroutine: s32'cmnCoolDown_DamageRate'
    endIf: 
    upon: (HIT_OR_GUARD)
      ifNot: Mem(ActReg3)
        damage: 500
        attackComboForceHitEx: 650
        storeValue: Mem(ActReg3), Val(1)
        pushboxFix: 1
      endIf: 
      else: 
        damage: 100
        attackComboForceHitEx: 150
        hitAirPushbackX: -2000
        clearUpon: (HIT_OR_GUARD)
      endElse: 
      ifNot: Mem(ObjReg1)
        storeValue: Mem(ObjReg1), Val(1)
        damageEffect: 2, s32'CHA_407_HitEff_D'
        sendSignalToAction: s32'CHA_407_Slash01', 24
      endIf: 
      else: 
        storeValue: Mem(ObjReg1), Val(0)
        damageEffect: 2, s32'CHA_407_HitEff2_D'
        sendSignalToAction: s32'CHA_407_Slash01', (SIGNAL_A)
      endElse: 
    endUpon: 
    storeValue: Mem(ActReg1), Val(1)
    upon: (HIT)
      modifyVar: (ADD), Mem(ActReg0), Val(1)
      if: Mem(ActReg1)
        ifOperation: (IS_EQUAL), Mem(ActReg0), Val(2)
          if: Mem(ActReg2)
            invulnForce: 1
            gotoLabelRequests: s32'Lock'
          endIf: 
          else: 
            storeValue: Mem(ActReg1), Val(0)
            ifNot: Mem(ObjReg1)
              gotoLabelRequests: s32'Odd'
            endIf: 
            else: 
              gotoLabelRequests: s32'Even'
            endElse: 
          endElse: 
        endIf: 
      endIf: 
      else: 
        ifOperation: (IS_EQUAL), Mem(ActReg0), Val(5)
          clearUpon: (HIT)
          ifNot: Mem(ObjReg1)
            gotoLabelRequests: s32'Odd2Finish'
          endIf: 
          else: 
            gotoLabelRequests: s32'Even2FInish'
          endElse: 
        endIf: 
      endElse: 
    endUpon: 
    upon: 78
      clearUpon: 78
      ifOperation: (IS_LESSER_OR_EQUAL), Mem(EnemyDistanceX), Val(400000)
        ifOperation: (IS_LESSER_OR_EQUAL), Mem(EnemyDistanceY), Val(500000)
          clearUpon: 78
          storeValue: Mem(ActReg2), Val(1)
          hitAirPushbackX: 0
        endIf: 
      endIf: 
    endUpon: 
  endUpon: 
  sprite: s32'cha407_00', 4
  sprite: s32'cha407_00', 2
  physicsXImpulse: 21000
  sprite: s32'cha407_01', 6
