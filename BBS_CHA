  checkLastStateName: s32'Hooligan_C'
  if: Mem(Tmp)
    storeValue: Mem(ActReg0), Val(1)
    callSubroutine: s32'cmnSpecialAttackC_AtkInit'
    callSubroutine: s32'cmnSpecialAttackGauge_C'
