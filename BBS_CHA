beginSubroutine: s32'OnActionBegin'
  checkSomething: 3, 3
  if: Mem(Tmp)
    Unknown1924: 0, 1
    Unknown1924: 1, 1
    Unknown1924: 2, 1
    Unknown1924: 3, 1
  endIf: 
  else: 
    Unknown1924: 0, 0
    Unknown1924: 1, 0
    Unknown1924: 2, 0
    Unknown1924: 3, 0
  endElse: 
endSubroutine: 

