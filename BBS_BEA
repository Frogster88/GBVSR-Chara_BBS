            storeValue: Mem(287), Val(20)
            Unknown1923: 0
        endUpon: 
    endIf: 
    checkCurrentStateName: (SELF), s32'SpecialShot_B'
    if: Mem(Tmp)
        upon: (BEFORE_EXIT)
            storeValue: Mem(287), Val(20)
            Unknown1923: 0
        endUpon: 
    endIf: 
