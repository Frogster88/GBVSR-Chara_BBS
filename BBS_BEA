  callSubroutine: s32'cmnTest'
  ifNot: Mem(PlayerVal0)
    if: Mem(435)
      ifOperation: (IS_GREATER_OR_EQUAL), Mem(HitPoint), Val(1)
        storeValue: Mem(PlayerVal0), Val(1)
        storeValue: Mem(ExGage0), Val(0)
        storeValue: Mem(ExGage1), Val(3900)
      endIf: 
    endIf: 
  endIf: 
  else: 
