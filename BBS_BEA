      Unknown1889: 1, 1
      storeValue: Mem(PlayerVal1), Val(0)
    endIf: 
    else: 
      Unknown1889: 1, 5
    endElse: 
    ifNot: Mem(435)
      ifNot: Mem(IsTrainingMode)
        storeValue: Mem(PlayerVal0), Val(0)
        storeValue: Mem(ExGage1), Val(0)
      endIf: 
      else: 
        getTrainingItemVal: s32'ETMI_UniqueGauge_BEA_COD', 2, 61
        ifOperation: (IS_NOT_EQUAL), Mem(GTmpReg0), Val(1)
          storeValue: Mem(PlayerVal0), Val(0)
          storeValue: Mem(ExGage1), Val(0)
        endIf: 
      endIf: 
    endElse: 
  endIf: 
endElse: 

