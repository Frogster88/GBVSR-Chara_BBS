beginSubroutine: s32'OnActionBegin'
  checkSomething: 3, 3
  if: Mem(Tmp)
    Unknown1924: 0, 1
    Unknown1924: 1, 1
    Unknown1924: 2, 1
    Unknown1924: 3, 1
  endIf: 
  else: 
    Unknown1924: 0, 0
    Unknown1924: 1, 0
    Unknown1924: 2, 0
    Unknown1924: 3, 0
  endElse: 
  checkCurrentStateName: (SELF), s32'SpecialShot_C'
  if: Mem(Tmp)
    callSubroutine: s32'Adversity_Check'
  endIf: 
  checkCurrentStateName: (SELF), s32'AntiAir_C'
  if: Mem(Tmp)
    callSubroutine: s32'Adversity_Check'
  endIf: 
  checkCurrentStateName: (SELF), s32'Tosshin_C'
  if: Mem(Tmp)
    callSubroutine: s32'Adversity_Check'
  endIf: 
  checkCurrentStateName: (SELF), s32'Assault_C'
  if: Mem(Tmp)
    callSubroutine: s32'Adversity_Check'
  endIf: 
endSubroutine: 

beginSubroutine: s32'Adversity_Check'
  if: Mem(ExGage0)
    storeValue: Mem(ActReg7), Val(1)
  endIf: 
  else: 
    ifOperation: (IS_GREATER_OR_EQUAL), Mem(ExGage1), Val(3900)
      storeValue: Mem(ActReg7), Val(1)
    endIf: 
  endElse: 
endSubroutine: 

beginSubroutine: s32'OnFinalize'
  callSubroutine: s32'cmn_FN_OnFinalize'
endSubroutine: 

beginSubroutine: s32'OnIdling'
  ifOperation: (IS_EQUAL), Mem(ActionTime), Val(3)
    callSubroutine: s32'AbilityCoolDown_N'
    callSubroutine: s32'AbilityCoolDown_N_ON_FINALIZE'
    callSubroutine: s32'AbilityCoolDown_6'
    callSubroutine: s32'AbilityCoolDown_4'
    callSubroutine: s32'AbilityCoolDown_2'
    checkCurrentStateName: (SELF), s32'SpecialShot_C'
    if: Mem(Tmp)
      callSubroutine: s32'Adversity_Reset'
    endIf: 
    checkCurrentStateName: (SELF), s32'AntiAir_C'
    if: Mem(Tmp)
      callSubroutine: s32'Adversity_Reset'
    endIf: 
    checkCurrentStateName: (SELF), s32'Tosshin_C'
    if: Mem(Tmp)
      callSubroutine: s32'Adversity_Reset'
    endIf: 
    checkCurrentStateName: (SELF), s32'Assault_C'
    if: Mem(Tmp)
      callSubroutine: s32'Adversity_Reset'
    endIf: 
  endIf: 
endSubroutine: 

beginSubroutine: s32'Adversity_Reset'
  if: Mem(ActReg7)
    storeValue: Mem(ExGage0), Val(0)
    storeValue: Mem(ExGage1), Val(0)
    createObject: s32'BEA_GyakkyoIConEff', (CENTER)
  endIf: 
endSubroutine: 

beginSubroutine: s32'OnDamage'
  callSubroutine: s32'cmn_Fn_OnDamage'
  ifOperation: (IS_LESSER_OR_EQUAL), Mem(ActionTime), Val(2)
    callSubroutine: s32'AbilityCoolDown_N_DAMAGE'
    callSubroutine: s32'AbilityCoolDown_6'
    callSubroutine: s32'AbilityCoolDown_4'
    callSubroutine: s32'AbilityCoolDown_2'
  endIf: 
endSubroutine: 

beginSubroutine: s32'OnEnemyComboBreak'
  storeValue: Mem(PlayerVal1), Val(0)
  storeValue: Mem(PlayerVal3), Val(0)
endSubroutine: 

beginSubroutine: s32'OnDie'
  storeValue: Mem(PlayerVal0), Val(0)
  storeValue: Mem(ExGage0), Val(0)
  storeValue: Mem(ExGage1), Val(0)
endSubroutine: 

