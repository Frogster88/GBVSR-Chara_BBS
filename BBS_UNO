beginSubroutine: s32'SpecialShot_Interrupt_Matome'
  upon: (HIT)
    clearUpon: (HIT)
    upon: 36
      checkInput: (INPUT_PRESS_L)
      if: Mem(Tmp)
        storeValue: Mem(ActReg0), Val(1)
      endIf: 
      checkInput: (INPUT_PRESS_M)
      if: Mem(Tmp)
        storeValue: Mem(ActReg0), Val(1)
      endIf: 
      checkInput: (INPUT_PRESS_H)
      if: Mem(Tmp)
        storeValue: Mem(ActReg0), Val(1)
      endIf: 
      checkInput: (INPUT_PRESS_S)
      if: Mem(Tmp)
        storeValue: Mem(ActReg0), Val(1)
      endIf: 
    endUpon: 
    if: Mem(IsEnemysCutScene)
      storeValue: Mem(ActReg0), Val(1)
    endIf: 
  endUpon: 
  upon: 99
    clearUpon: 99
    storeValue: Mem(ActReg1), Val(1)
  endUpon: 
  upon: 8
    clearUpon: 8
    attackOff: 
  endUpon: 
endSubroutine: 

