beginSubroutine: s32'OnFrameStep'
  if: Mem(437)
    checkCurrentStateName: (SELF), s32'NmlAtk2A'
    if: Mem(Tmp)
      checkLastStateName: s32'NmlAtk2A'
      if: Mem(Tmp)
        upon: 135
          Unknown1631: s32'AntiAir_C_SA'
        endUpon: 
        upon: 136
          Unknown1631: s32'Assault_A_SA'
        endUpon: 
      endIf: 
    endIf: 
    checkCurrentStateName: (SELF), s32'NmlAtk5A3rd'
    if: Mem(Tmp)
      upon: 135
        Unknown1631: s32'SpecialShot_C_SA'
        Unknown1631: s32'SpecialShot_D_SA'
        Unknown1631: s32'Assault_C_SA'
        Unknown1631: s32'Assault_D_SA'
        Unknown1631: s32'uno_Ougi_SA'
        Unknown1631: s32'uno_Kaihou_SA'
      endUpon: 
      upon: 136
