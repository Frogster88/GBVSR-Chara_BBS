    disableHitstopForSelf: 0
    attackFrontDirection: 0
    forceHitSignal: 1
    attackIgnoreCaution: 1
    invulnForce: 0
    Unknown2204: 1
    upon: (ON_DESTROY)
        clearUpon: (FRAME_STEP)
        clearUpon: 35
        clearUpon: 38
        clearUpon: 12
        gotoLabelRequests: s32'koware'
        stopLinkObject: 0
        ignoreSuperFreeze: 1
        Unknown2050: 1
    endUpon: 
    upon: 35
        checkScreenCollision: 0
        setNoCollision: 0
        clearUpon: (FRAME_STEP)
        clearUpon: (ON_DESTROY)
        clearUpon: 35
        clearUpon: 38
        clearUpon: 12
        gotoLabelRequests: s32'end'
        stopLinkObject: 0
        ignoreSuperFreeze: 1
        Unknown2050: 1
    endUpon: 
    upon: 38
        checkScreenCollision: 0
        setNoCollision: 0
        clearUpon: (FRAME_STEP)
        clearUpon: (ON_DESTROY)
        clearUpon: 35
        clearUpon: 38
        clearUpon: 12
        gotoLabelRequests: s32'end'
        stopLinkObject: 0
        ignoreSuperFreeze: 1
        Unknown2050: 1
    endUpon: 
endSubroutine: 

