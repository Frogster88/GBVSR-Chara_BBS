beginSubroutine: s32'OnIdling'
  if: Mem(ObjReg4)
    modifyVar: (ADD), Mem(ObjReg4), Val(-1)
    ifNot: Mem(ObjReg4)
      Unknown413: 0, 1, 11, 0
    endIf: 
  endIf: 
  ifOperation: (IS_EQUAL), Mem(ActionTime), Val(3)
    callSubroutine: s32'AbilityCoolDown_N'
    callSubroutine: s32'AbilityCoolDown_N_ON_FINALIZE'
    callSubroutine: s32'AbilityCoolDown_6'
    callSubroutine: s32'AbilityCoolDown_6_ON_FINALIZE'
    callSubroutine: s32'AbilityCoolDown_4'
    callSubroutine: s32'AbilityCoolDown_2'
    callSubroutine: s32'AbilityCoolDown_2_ON_FINALIZE'
  endIf: 
endSubroutine: 

beginSubroutine: s32'SetMatParamSpeed10_5F_Strike'
  Unknown412: 0, 1, 11, 0
  Unknown413: 10, 1, 11, 0
  storeValue: Mem(ObjReg4), Val(6)
endSubroutine: 

beginSubroutine: s32'OnFrameStep'
  callSubroutine: s32'cmnTest'
  checkSomething: 3, 3
  if: Mem(Tmp)
    Unknown1924: 2, 1
  endIf: 
  checkSomething: 3, 3
  ifNot: Mem(Tmp)
    ifNot: Mem(PlayerVal0)
      isObjectActive: (STACK0)
      if: Mem(Tmp)
        Unknown1924: 2, 1
      endIf: 
      else: 
        Unknown1924: 2, 0
      endElse: 
    endIf: 
  endIf: 
  ifNot: Mem(PlayerVal6)
    ifOperation: (IS_EQUAL), Mem(PlayerVal3), Val(99999)
      storeValue: Mem(PlayerVal3), Val(0)
    endIf: 
    if: Mem(EnvR21)
      storeValue: Mem(EnvR21), Val(0)
    endIf: 
  endIf: 
  if: Mem(PlayerDamage)
    checkCurrentStateName: (SELF), s32'CmnActHajikareStand'
    ifNot: Mem(Tmp)
      checkCurrentStateName: (SELF), s32'CmnActHajikareCrouch'
      ifNot: Mem(Tmp)
        checkCurrentStateName: (SELF), s32'CmnActHajikareAir'
        ifNot: Mem(Tmp)
          checkCurrentStateName: (SELF), s32'CmnActLockWait'
          ifNot: Mem(Tmp)
            checkCurrentStateName: (SELF), s32'CmnActAirLockReject'
            ifNot: Mem(Tmp)
              sendSignalToAction: s32'eus402_BombD_Atk', 32
            endIf: 
          endIf: 
        endIf: 
      endIf: 
    endIf: 
  endIf: 
endSubroutine: 

beginSubroutine: s32'OnActionBegin'
  callSubroutine: s32'cmn_FN_OnActionBegin'
  checkSomething: 3, 3
  if: Mem(Tmp)
    Unknown1924: 0, 1
    Unknown1924: 1, 1
    Unknown1924: 2, 1
    Unknown1924: 3, 1
  endIf: 
  else: 
    Unknown1924: 0, 0
    Unknown1924: 1, 0
    Unknown1924: 2, 0
    Unknown1924: 3, 0
  endElse: 
  if: Mem(PlayerVal0)
    Unknown1924: 2, 1
  endIf: 
endSubroutine: 

beginSubroutine: s32'OnFinalize'
  callSubroutine: s32'cmn_FN_OnFinalize'
  if: Mem(PlayerVal0)
    Unknown1924: 2, 0
  endIf: 
  Unknown412: 0, 1, 9, 0
  Unknown413: 0, 1, 9, 0
  Unknown412: 0, 1, 10, 0
  Unknown413: 0, 1, 10, 0
  Unknown412: 0, 1, 11, 0
  Unknown413: 0, 1, 11, 0
endSubroutine: 

beginSubroutine: s32'Func_Explosion'
  Unknown2349: 23
  upon: (HIT_OR_GUARD)
    sendSignalToAction: s32'eus402_BombC_Set_Atk', 24
  endUpon: 
endSubroutine: 

beginSubroutine: s32'OnDamage'
  callSubroutine: s32'cmn_Fn_OnDamage'
  ifOperation: (IS_LESSER_OR_EQUAL), Mem(ActionTime), Val(2)
    callSubroutine: s32'AbilityCoolDown_N_DAMAGE'
    callSubroutine: s32'AbilityCoolDown_6_DAMAGE'
    callSubroutine: s32'AbilityCoolDown_4'
    callSubroutine: s32'AbilityCoolDown_2_DAMAGE'
  endIf: 
endSubroutine: 

beginSubroutine: s32'OnLanding'
  callSubroutine: s32'cmn_FN_OnLanding'
endSubroutine: 

beginSubroutine: s32'AbilityCoolDown_N_DAMAGE'
  checkCurrentStateName: (SELF), s32'AntiAir_A'
  if: Mem(Tmp)
    storeValue: Mem(287), Val(20)
    Unknown1923: 0
  endIf: 
  checkCurrentStateName: (SELF), s32'AntiAir_B'
  if: Mem(Tmp)
    storeValue: Mem(287), Val(20)
    Unknown1923: 0
  endIf: 
  checkCurrentStateName: (SELF), s32'AntiAir_C'
  if: Mem(Tmp)
    storeValue: Mem(287), Val(420)
    Unknown1923: 0
  endIf: 
  checkCurrentStateName: (SELF), s32'AntiAir_D'
  if: Mem(Tmp)
    if: Mem(287)
      modifyVar2: (ADD), Val(180), Mem(287)
      storeValue: Mem(287), Mem(Tmp)
    endIf: 
    else: 
      storeValue: Mem(287), Val(180)
    endElse: 
    Unknown1923: 0
  endIf: 
endSubroutine: 

