        storeValue: Mem(ActReg6), Val(1)
    endIf: 
    ifPlayerCharacter: s16'per'
        storeValue: Mem(ActReg6), Val(1)
    endIf: 
    ifPlayerCharacter: s16'low'
        storeValue: Mem(ActReg6), Val(1)
    endIf: 
