        storeValue: Mem(ActReg6), Val(1)
    endIf: 
    storeValue: Mem(ActReg7), Val(14)
    ifPlayerCharacter: s16'ctl'
        storeValue: Mem(ActReg7), Val(10)
    endIf: 
