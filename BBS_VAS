        endIf: 
        ifNot: Mem(Tmp)
            checkCurrentStateName: (SELF), s32'kamae_Add_B'
        endIf: 
        ifNot: Mem(Tmp)
            checkCurrentStateName: (SELF), s32'kamae_Add_C'
        endIf: 
        ifNot: Mem(Tmp)
            checkCurrentStateName: (SELF), s32'kamae_Add_D'
        endIf: 
        ifNot: Mem(Tmp)
            storeValue: Mem(ExGage0), Val(2)
            ifOperation: (IS_LESSER_OR_EQUAL), Mem(ExGage1), Val(90)
                storeValue: Mem(ExGage1), Val(1)
            endIf: 
            else: 
                modifyVar: (ADD), Mem(ExGage1), Val(-90)
            endElse: 
        endIf: 
    endIf: 
    if: Mem(ExGage0)
        storeValue: Mem(PlayerVal1), Val(1)
        callSubroutine: s32'cmnArmor_HighMidLow'
        superArmorHitstop: -2, -1
        superArmorDamagePercent: 50
        disableThrow: 1
        upon: 41
            setLightRedColor: 400
            setLightGreenColor: 0
            setLightBlueColor: 400
            setLightRedColorSpeed: -80
            setLightGreenColorSpeed: -80
            setLightBlueColorSpeed: -80
            if: Mem(ActReg7)
                storeValue: Mem(ActReg6), Val(3)
            endIf: 
            modifyVar: (ADD), Mem(ExGage0), Val(-1)
            ifNot: Mem(ExGage0)
                clearUpon: 41
                callSubroutine: s32'cmnMuteki_False'
                storeValue: Mem(PlayerVal1), Val(0)
            endIf: 
            ifOperation: (IS_LESSER_OR_EQUAL), Mem(ExGage1), Val(90)
                storeValue: Mem(ExGage1), Val(1)
            endIf: 
            else: 
                modifyVar: (ADD), Mem(ExGage1), Val(-90)
            endElse: 
        endUpon: 
        upon: (FRAME_STEP)
            if: Mem(ActReg7)
                if: Mem(ActReg6)
                    modifyVar: (ADD), Mem(ActReg6), Val(-1)
                    ifOperation: (IS_LESSER_OR_EQUAL), Mem(ActReg6), Val(0)
                        storeValue: Mem(ActReg6), Val(0)
                        setLightRedColor: 0
                        setLightGreenColor: 0
                        setLightBlueColor: 0
