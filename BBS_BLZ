beginSubroutine: s32'OnFinalize'
    callSubroutine: s32'cmn_FN_OnFinalize'
endSubroutine: 

beginSubroutine: s32'OnIdling'
    ifOperation: (IS_EQUAL), Mem(ActionTime), Val(3)
        callSubroutine: s32'AbilityCoolDown_N'
        callSubroutine: s32'AbilityCoolDown_N_ON_FINALIZE'
        callSubroutine: s32'AbilityCoolDown_N_Air'
        callSubroutine: s32'AbilityCoolDown_6'
        callSubroutine: s32'AbilityCoolDown_4'
        callSubroutine: s32'AbilityCoolDown_2'
        callSubroutine: s32'AbilityCoolDown_2_ON_FINALIZE'
    endIf: 
endSubroutine: 

beginSubroutine: s32'OnDamage'
    callSubroutine: s32'cmn_Fn_OnDamage'
    ifOperation: (IS_LESSER_OR_EQUAL), Mem(ActionTime), Val(2)
        callSubroutine: s32'AbilityCoolDown_N_DAMAGE'
        callSubroutine: s32'AbilityCoolDown_N_Air'
        callSubroutine: s32'AbilityCoolDown_6'
        callSubroutine: s32'AbilityCoolDown_4'
        callSubroutine: s32'AbilityCoolDown_2_DAMAGE'
    endIf: 
endSubroutine: 

beginSubroutine: s32'OnLanding'
    callSubroutine: s32'cmn_FN_OnLanding'
endSubroutine: 

beginSubroutine: s32'AbilityCoolDown_N_DAMAGE'
    checkCurrentStateName: (SELF), s32'SpecialShot_A'
    if: Mem(Tmp)
        storeValue: Mem(287), Val(20)
        Unknown1923: 0
    endIf: 
    checkCurrentStateName: (SELF), s32'SpecialShot_B'
    if: Mem(Tmp)
        storeValue: Mem(287), Val(20)
        Unknown1923: 0
    endIf: 
    checkCurrentStateName: (SELF), s32'SpecialShot_C'
    if: Mem(Tmp)
        storeValue: Mem(287), Val(420)
        Unknown1923: 0
    endIf: 
    checkCurrentStateName: (SELF), s32'SpecialShot_D'
    if: Mem(Tmp)
        if: Mem(287)
            modifyVar2: (ADD), Val(180), Mem(287)
            storeValue: Mem(287), Mem(Tmp)
        endIf: 
        else: 
            storeValue: Mem(287), Val(180)
        endElse: 
        Unknown1923: 0
    endIf: 
endSubroutine: 

beginSubroutine: s32'AbilityCoolDown_2_DAMAGE'
    checkCurrentStateName: (SELF), s32'BlackHole_A'
    if: Mem(Tmp)
        storeValue: Mem(289), Val(240)
        Unknown1923: 2
    endIf: 
    checkCurrentStateName: (SELF), s32'BlackHole_B'
    if: Mem(Tmp)
        storeValue: Mem(289), Val(240)
        Unknown1923: 2
    endIf: 
    checkCurrentStateName: (SELF), s32'BlackHole_C'
    if: Mem(Tmp)
        storeValue: Mem(289), Val(600)
        Unknown1923: 2
    endIf: 
    checkCurrentStateName: (SELF), s32'BlackHole_D'
    if: Mem(Tmp)
        if: Mem(289)
            modifyVar2: (ADD), Val(300), Mem(289)
            storeValue: Mem(289), Mem(Tmp)
        endIf: 
        else: 
            storeValue: Mem(289), Val(300)
        endElse: 
        Unknown1923: 2
    endIf: 
endSubroutine: 

beginSubroutine: s32'AbilityCoolDown_N_ON_FINALIZE'
    checkCurrentStateName: (SELF), s32'SpecialShot_A'
    if: Mem(Tmp)
        upon: (BEFORE_EXIT)
            storeValue: Mem(287), Val(20)
            Unknown1923: 0
        endUpon: 
    endIf: 
    checkCurrentStateName: (SELF), s32'SpecialShot_B'
