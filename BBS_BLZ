beginSubroutine: s32'AbilityCoolDown_N'
    checkCurrentStateName: (SELF), s32'SpecialShot_A'
    if: Mem(Tmp)
        skillCooldown: 0, 999
        Unknown1923: 0
    endIf: 
    checkCurrentStateName: (SELF), s32'SpecialShot_B'
    if: Mem(Tmp)
        skillCooldown: 0, 999
        Unknown1923: 0
    endIf: 
    checkCurrentStateName: (SELF), s32'SpecialShot_C'
    if: Mem(Tmp)
        skillCooldown: 0, 999
        Unknown1923: 0
    endIf: 
    checkCurrentStateName: (SELF), s32'SpecialShot_D'
    if: Mem(Tmp)
        storeValue: Mem(ObjReg6), Val(0)
        if: Mem(287)
            storeValue: Mem(ObjReg6), Val(1)
            storeValue: Mem(417), Val(1)
            modifyVar2: (ADD), Val(60), Mem(287)
            storeValue: Mem(287), Mem(Tmp)
        endIf: 
        else: 
            skillCooldown: 0, 999
        endElse: 
        Unknown1923: 0
    endIf: 
endSubroutine: 

beginSubroutine: s32'AbilityCoolDown_N_Air'
    checkCurrentStateName: (SELF), s32'Assault_Air_A'
    if: Mem(Tmp)
        skillCooldown: 1, 20
        Unknown1923: 1
    endIf: 
    checkCurrentStateName: (SELF), s32'Assault_Air_B'
    if: Mem(Tmp)
        skillCooldown: 1, 20
        Unknown1923: 1
    endIf: 
    checkCurrentStateName: (SELF), s32'Assault_Air_C'
    if: Mem(Tmp)
        skillCooldown: 1, 420
        Unknown1923: 1
    endIf: 
    checkCurrentStateName: (SELF), s32'Assault_Air_D'
    if: Mem(Tmp)
        if: Mem(288)
            storeValue: Mem(417), Val(1)
        endIf: 
        modifyVar2: (ADD), Val(180), Mem(288)
        storeValue: Mem(288), Mem(Tmp)
        Unknown1923: 1
    endIf: 
endSubroutine: 

