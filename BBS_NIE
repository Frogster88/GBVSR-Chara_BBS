beginSubroutine: s32'ChainMatome'
  callSubroutine: s32'AddFlexChain_236'
  callSubroutine: s32'AddFlexChain_623'
  callSubroutine: s32'AddFlexChain_214'
  callSubroutine: s32'AddFlexChain_22'
  bufferSpecialInput: 15
  checkCurrentStateName: (SELF), s32'SpecialMultiAttack_A'
  if: Mem(Tmp)
    callSubroutine: s32'DelFlexChain_236'
  endIf: 
  checkCurrentStateName: (SELF), s32'SpecialMultiAttack_B'
  if: Mem(Tmp)
    callSubroutine: s32'DelFlexChain_236'
  endIf: 
  checkCurrentStateName: (SELF), s32'SpecialMultiAttack_C'
  if: Mem(Tmp)
    callSubroutine: s32'DelFlexChain_236'
  endIf: 
  checkCurrentStateName: (SELF), s32'AntiAir_A'
  if: Mem(Tmp)
    callSubroutine: s32'DelFlexChain_623'
  endIf: 
  checkCurrentStateName: (SELF), s32'AntiAir_B'
  if: Mem(Tmp)
    callSubroutine: s32'DelFlexChain_623'
  endIf: 
  checkCurrentStateName: (SELF), s32'AntiAir_C'
  if: Mem(Tmp)
    callSubroutine: s32'DelFlexChain_623'
  endIf: 
  checkCurrentStateName: (SELF), s32'Assault_A'
  if: Mem(Tmp)
    callSubroutine: s32'DelFlexChain_214'
  endIf: 
  checkCurrentStateName: (SELF), s32'Assault_B'
  if: Mem(Tmp)
    callSubroutine: s32'DelFlexChain_214'
  endIf: 
  checkCurrentStateName: (SELF), s32'Assault_C'
  if: Mem(Tmp)
    callSubroutine: s32'DelFlexChain_214'
  endIf: 
  checkCurrentStateName: (SELF), s32'SpecialAttack_A'
  if: Mem(Tmp)
    callSubroutine: s32'DelFlexChain_22'
  endIf: 
  checkCurrentStateName: (SELF), s32'SpecialAttack_B'
  if: Mem(Tmp)
    callSubroutine: s32'DelFlexChain_22'
  endIf: 
  checkCurrentStateName: (SELF), s32'SpecialAttack_C'
  if: Mem(Tmp)
    callSubroutine: s32'DelFlexChain_22'
  endIf: 
endSubroutine: 

beginSubroutine: s32'LoopEnd'
  upon: (FRAME_STEP)
    ifOperation: (IS_GREATER_OR_EQUAL), Mem(ActReg2), Val(1)
      modifyVar: (ADD), Mem(ActReg2), Val(-1)
    endIf: 
    ifOperation: (IS_GREATER), Mem(ActionTime), Mem(ActReg1)
      ifNot: Mem(ActReg5)
        clearUpon: (FRAME_STEP)
        enableWhiffCancel: 0
        attackEndDelay: 
        bufferSpecialInput: 0
        gotoLabelRequests: s32'End'
      endIf: 
    endIf: 
  endUpon: 
  setEventTrigger: 13, 41
  upon: 13
    clearUpon: 13
    recoveryState: 
  endUpon: 
endSubroutine: 

beginSubroutine: s32'CreateCard'
  smartRandomVoiceLine: s16'vNIE_ATKS_00', 100, s16'', 0, s16'', 0, s16'', 0
