beginState: s32'SpecialMultiAttack_A'
  upon: (IMMEDIATE)
    callSubroutine: s32'cmnSpecialAttack_Init'
    callSubroutine: s32'ChainMatome'
    storeValue: Mem(ActReg1), Val(55)
    callSubroutine: s32'LoopEnd'
    attackIgnoreCaution: 1
  endUpon: 
  sprite: s32'nie400_00', 2
  sprite: s32'nie400_00', 1
  ifNot: Mem(PlayerVal0)
    createObject: s32'dea401_A', (PLAYER)
    stackObject: (STACK0), (PREVIOUS)
  endIf: 
  else: 
    pawnAnimation: 4, s32'dea401_A', 0
  endElse: 
  callSubroutine: s32'CreateCard'
  callSubroutine: s32'Test_Icon_LM'
  Unknown1681: 
  sprite: s32'nie400_01', 3
  createObject: s32'nie_400_Icon01Eff', (PLAYER)
  sprite: s32'nie400_02', 3
  enableWhiffCancel: 1
  sprite: s32'nie400_03', 3
  sprite: s32'nie400_04', 3
  createObject: s32'nie_400_JewelRayEff', (PLAYER)
