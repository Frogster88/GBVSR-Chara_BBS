beginSubroutine: s32'CountDown'
  modifyVar: (ADD), Mem(ActReg0), Val(1)
  ifOperation: (IS_EQUAL), Mem(ActReg0), Val(1)
    modifyVar: (ADD), Mem(ActReg1), Val(-10)
  endIf: 
  ifOperation: (IS_EQUAL), Mem(ActReg0), Val(2)
    modifyVar: (ADD), Mem(ActReg1), Val(-25)
  endIf: 
  ifOperation: (IS_EQUAL), Mem(ActReg0), Val(3)
    modifyVar: (ADD), Mem(ActReg1), Val(-30)
  endIf: 
  ifOperation: (IS_NOT_EQUAL), Mem(ObjReg3), Val(0)
    ifOperation: (IS_EQUAL), Mem(ObjReg3), Val(74)
      modifyVar: (ADD), Mem(ActReg1), Val(-4)
      storeValue: Mem(ObjReg3), Val(0)
    endIf: 
  endIf: 
  deactivateObjectByName: s32'nie_400_Icon01Eff'
  deactivateObjectByName: s32'nie_400_Icon02Eff'
  deactivateObjectByName: s32'nie_400_Icon03Eff'
  deactivateObjectByName: s32'nie_400_Icon04Eff'
  modifyVar: (ADD), Mem(ExGage0), Val(-1)
endSubroutine: 

