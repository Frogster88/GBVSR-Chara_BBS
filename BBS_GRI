            attackOff: 
            storeValue: Mem(ActReg3), Val(0)
          endIf: 
        endIf: 
        modifyVar: (ADD), Mem(ActReg2), Val(-1)
        ifNot: Mem(ActReg2)
          clearUpon: (HIT_OR_GUARD)
          clearUpon: (FRAME_STEP)
          storeValue: Mem(ActReg1), Val(0)
