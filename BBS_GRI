beginSubroutine: s32'OnFrameStep'
  callSubroutine: s32'cmnTest'
  if: Mem(IsTrainingMode)
    if: Mem(BattleDoing)
      if: Mem(ExGage0)
        getTrainingItemVal: s32'ETMI_UniqueGauge_GRI_Tenyu', 2, 61
        ifOperation: (IS_EQUAL), Mem(GTmpReg0), Val(1)
          if: Mem(IsRecoverableTypeA)
            storeValue: Mem(ExGage0), Val(99999)
          endIf: 
          ifOperation: (IS_LESSER_OR_EQUAL), Mem(ExGage0), Val(10)
            storeValue: Mem(ExGage0), Val(10)
          endIf: 
        endIf: 
      endIf: 
    endIf: 
  endIf: 
  if: Mem(ExGage0)
    ifNot: Mem(DebugSkillEx)
      ifNot: Mem(295)
        checkCurrentStateName: (SELF), s32'gri_Ougi_Buff'
        ifNot: Mem(Tmp)
          if: Mem(BattleDoing)
            modifyVar: (SUB), Mem(ExGage0), Val(3)
          endIf: 
          else: 
            storeValue: Mem(ExGage0), Val(0)
          endElse: 
        endIf: 
        ifOperation: (IS_GREATER_OR_EQUAL), Mem(ObjReg2), Val(12)
          ifNot: Mem(DebugSkillEx)
            Unknown2268: 0, 800
            createParticleWithArg2: s32'gri_431_BuffAura', (HEAD_3D)
            Unknown2268: 0, 1200
            createParticleWithArg2: s32'gri_431_BuffAura', (CHEST_3D)
            Unknown2268: 0, 1200
            createParticleWithArg2: s32'gri_431_BuffAura', (CALF_L_3D)
            Unknown2268: 0, 1200
            createParticleWithArg2: s32'gri_431_BuffAura', (CALF_R_3D)
            Unknown2268: 0, 800
            createParticleWithArg2: s32'gri_431_BuffAura', (HAND_L_3D)
            Unknown2268: 0, 800
            createParticleWithArg2: s32'gri_431_BuffAura', (HAND_R_3D)
            Unknown2268: 0, 800
            createParticleWithArg2: s32'gri_431_BuffAura', (ELBOW_L_3D)
            Unknown2268: 0, 800
            createParticleWithArg2: s32'gri_431_BuffAura', (ELBOW_R_3D)
            Unknown2268: 0, 800
            createParticleWithArg2: s32'gri_431_BuffAura', (FOOT_L_3D)
            Unknown2268: 0, 800
            createParticleWithArg2: s32'gri_431_BuffAura', (FOOT_R_3D)
            storeValue: Mem(ObjReg2), Val(0)
          endIf: 
        endIf: 
        else: 
          modifyVar: (ADD), Mem(ObjReg2), Val(1)
        endElse: 
      endIf: 
    endIf: 
    ifOperation: (IS_GREATER), Mem(ExGage0), Val(1)
      tensionCooldown: 300
    endIf: 
  endIf: 
  if: Mem(PlayerVal5)
    storeValue: Mem(ObjReg3), Val(0)
  endIf: 
  if: Mem(IsLand)
    storeValue: Mem(PlayerVal1), Val(0)
    checkCurrentStateName: (SELF), s32'Booster_Move_Land'
    ifNot: Mem(Tmp)
      checkCurrentStateName: (SELF), s32'Booster_Move_Air'
      ifNot: Mem(Tmp)
        storeValue: Mem(PlayerVal2), Val(0)
      endIf: 
    endIf: 
  endIf: 
  if: Mem(ObjReg4)
    checkCurrentStateName: (SELF), s32'NmlAtk5D'
    ifNot: Mem(Tmp)
      checkCurrentStateName: (SELF), s32'Booster_Move_Land'
      ifNot: Mem(Tmp)
        checkCurrentStateName: (SELF), s32'Booster_Move_Air'
        ifNot: Mem(Tmp)
          storeValue: Mem(ObjReg4), Val(0)
        endIf: 
      endIf: 
    endIf: 
  endIf: 
  checkMoveFlag: 60
  if: Mem(Tmp)
    checkSomething: 3, 1
    if: Mem(Tmp)
      checkCurrentStateName: (SELF), s32'ThrowExe'
      ifNot: Mem(Tmp)
        checkCurrentStateName: (SELF), s32'BackThrowExe'
        ifNot: Mem(Tmp)
          checkCurrentStateName: (SELF), s32'AirThrowExe'
          ifNot: Mem(Tmp)
            checkCurrentStateName: (SELF), s32'NmlAtk2D'
            ifNot: Mem(Tmp)
              checkCurrentStateName: (SELF), s32'NmlAtkDash6A'
              ifNot: Mem(Tmp)
                checkCurrentStateName: (SELF), s32'NmlAtkDash6B'
