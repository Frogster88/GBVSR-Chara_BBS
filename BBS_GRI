beginSubroutine: s32'Assault_Air_MultiDamage'
  checkCurrentStateName: (SELF), s32'Assault_Air_D'
  if: Mem(Tmp)
    if: Mem(ActReg0)
      damage: 200
    endIf: 
  endIf: 
  else: 
    if: Mem(ActReg0)
      damage: 100
      ifOperation: (IS_GREATER_OR_EQUAL), Mem(ActReg0), Val(5)
        damage: 50
      endIf: 
    endIf: 
  endElse: 
endSubroutine: 

beginSubroutine: s32'Assault_Air_Init'
  callSubroutine: s32'cmnSpecialAtkLv3_AtkInit'
  callSubroutine: s32'cmnSpecialAttack_Init'
  damage: 500
  hitstop: 3
  groundHitEffect: (AIR_LAUNCH)
  airHitEffect: (AIR_LAUNCH)
  hitAirPushbackX: 3000
  hitAirPushbackY: 8000
  wallPushback: 0, 1000
  clashLevel: 1
  isAirUnblockable: 0
  attackRange: 2000000, -100000, -1, -1
  damageEffect: 2, s32'gri_412_HitEff'
  upon: (HIT_OR_GUARD)
    callSubroutine: s32'Assault_Air_MultiDamage'
    modifyVar: (ADD), Mem(ActReg0), Val(1)
    ifOperation: (IS_GREATER_OR_EQUAL), Mem(EnemyDiffY), Val(0)
      ifOperation: (IS_LESSER_OR_EQUAL), Mem(SpeedY), Val(0)
        hitAirPushbackY: -8000
      endIf: 
      ifOperation: (IS_LESSER_OR_EQUAL), Mem(SpeedY), Val(-20000)
        hitAirPushbackY: -12000
      endIf: 
      ifOperation: (IS_LESSER_OR_EQUAL), Mem(SpeedY), Val(-30000)
        hitAirPushbackY: -15000
      endIf: 
    endIf: 
  endUpon: 
  upon: (TOUCH_GROUND)
    clearUpon: (FRAME_STEP)
    clearUpon: (TOUCH_GROUND)
    clearUpon: (HIT_OR_GUARD)
    gotoLabelRequests: s32'Landing'
  endUpon: 
  callSubroutine: s32'Func_PreSpecialJump'
endSubroutine: 

