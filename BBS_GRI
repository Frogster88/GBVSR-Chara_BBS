  endElse: 
  ifNot: Mem(ObjReg3)
    isObjectActive: (STACK1)
    if: Mem(Tmp)
      storeValueFromObject: (PLAYER), Mem(ObjReg1), (STACK1), Mem(PosX)
      modifyVar: (SUB), Mem(ObjReg1), Mem(PosX)
      ifOperation: (IS_LESSER_OR_EQUAL), Mem(ObjReg1), Val(140000)
        ifOperation: (IS_GREATER_OR_EQUAL), Mem(ObjReg1), Val(-140000)
