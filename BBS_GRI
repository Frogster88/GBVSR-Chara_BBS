beginState: s32'AntiAir_D'
  upon: (IMMEDIATE)
    callSubroutine: s32'AntiAir_Init'
    callSubroutine: s32'cmnSpecialAttackD_Init'
    callSubroutine: s32'cmnMuteki_SousaiLv_D'
    damage: 200
    hitAirPushbackX: 3000
    hitAirPushbackY: 20000
    hitstop: 2
    attackFrontDirection: 2
    attackRange: -1, -1, -1, -1
    upon: (HIT_OR_GUARD)
      ifNot: Mem(ActReg2)
        if: Mem(ActReg0)
          damage: 200
        endIf: 
        storeValue: Mem(ActReg2), Val(1)
      endIf: 
      else: 
        damage: 100
        clearUpon: (HIT_OR_GUARD)
      endElse: 
    endUpon: 
    upon: (HIT)
      clearUpon: (HIT)
      ifOperation: (IS_LESSER_OR_EQUAL), Mem(EnemyDistance), Val(400000)
        hitAirPushbackX: 3000
        hitAirPushbackY: 20000
        killType: (NOT_KILL)
        attackHitPositionType: 4, 200000, 20000
        storeValue: Mem(ActReg0), Val(1)
      endIf: 
      else: 
        hitAirPushbackX: -12000
        hitAirPushbackY: 30000
      endElse: 
    endUpon: 
