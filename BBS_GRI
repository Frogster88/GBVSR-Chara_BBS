                      checkCurrentStateName: (SELF), s32'GuardCrash'
                      ifNot: Mem(Tmp)
                        checkCurrentStateName: (SELF), s32'CancelDash'
                        ifNot: Mem(Tmp)
                        checkCurrentStateName: (SELF), s32'GC_Attack'
