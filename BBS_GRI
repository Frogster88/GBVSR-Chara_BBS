        ifOperation: (IS_GREATER_OR_EQUAL), Mem(ObjReg1), Val(-140000)
          storeValueFromObject: (PLAYER), Mem(ObjReg1), (STACK2), Mem(PosY)
          modifyVar: (SUB), Mem(ObjReg1), Mem(PosY)
          ifOperation: (IS_LESSER_OR_EQUAL), Mem(ObjReg1), Val(400000)
            ifOperation: (IS_GREATER_OR_EQUAL), Mem(ObjReg1), Val(50000)
              storeValueFromObject: (PLAYER), Mem(Tmp), (STACK2), Mem(ActReg0)
              ifNot: Mem(Tmp)
                storeValue: Mem(PlayerVal0), Val(2)
              endIf: 
            endIf: 
          endIf: 
        endIf: 
      endIf: 
    endIf: 
    isObjectActive: (STACK3)
    if: Mem(Tmp)
      storeValueFromObject: (PLAYER), Mem(ObjReg1), (STACK3), Mem(PosX)
      modifyVar: (SUB), Mem(ObjReg1), Mem(PosX)
      ifOperation: (IS_LESSER_OR_EQUAL), Mem(ObjReg1), Val(140000)
        ifOperation: (IS_GREATER_OR_EQUAL), Mem(ObjReg1), Val(-140000)
          storeValueFromObject: (PLAYER), Mem(ObjReg1), (STACK3), Mem(PosY)
          modifyVar: (SUB), Mem(ObjReg1), Mem(PosY)
          ifOperation: (IS_LESSER_OR_EQUAL), Mem(ObjReg1), Val(400000)
            ifOperation: (IS_GREATER_OR_EQUAL), Mem(ObjReg1), Val(50000)
              storeValueFromObject: (PLAYER), Mem(Tmp), (STACK3), Mem(ActReg0)
              ifNot: Mem(Tmp)
                storeValue: Mem(PlayerVal0), Val(3)
              endIf: 
            endIf: 
          endIf: 
        endIf: 
      endIf: 
    endIf: 
    isObjectActive: (STACK4)
    if: Mem(Tmp)
      storeValueFromObject: (PLAYER), Mem(ObjReg1), (STACK4), Mem(PosX)
      modifyVar: (SUB), Mem(ObjReg1), Mem(PosX)
      ifOperation: (IS_LESSER_OR_EQUAL), Mem(ObjReg1), Val(140000)
        ifOperation: (IS_GREATER_OR_EQUAL), Mem(ObjReg1), Val(-140000)
          storeValueFromObject: (PLAYER), Mem(ObjReg1), (STACK4), Mem(PosY)
          modifyVar: (SUB), Mem(ObjReg1), Mem(PosY)
          ifOperation: (IS_LESSER_OR_EQUAL), Mem(ObjReg1), Val(400000)
            ifOperation: (IS_GREATER_OR_EQUAL), Mem(ObjReg1), Val(50000)
              storeValueFromObject: (PLAYER), Mem(Tmp), (STACK4), Mem(ActReg0)
              ifNot: Mem(Tmp)
                storeValue: Mem(PlayerVal0), Val(4)
              endIf: 
            endIf: 
          endIf: 
        endIf: 
      endIf: 
    endIf: 
  endIf: 
  isObjectActive: (STACK1)
  if: Mem(Tmp)
    Unknown2035: 5, s32'Search_Icon', 0
    Unknown2035: 6, s32'NotSearch_Icon', 0
    Unknown2035: 7, s32'NotSearch_Icon', 0
    Unknown2035: 8, s32'NotSearch_Icon', 0
  endIf: 
  else: 
    isObjectActive: (STACK2)
    if: Mem(Tmp)
      Unknown2035: 6, s32'Search_Icon', 0
      Unknown2035: 7, s32'NotSearch_Icon', 0
      Unknown2035: 8, s32'NotSearch_Icon', 0
    endIf: 
    else: 
      isObjectActive: (STACK3)
      if: Mem(Tmp)
        Unknown2035: 7, s32'Search_Icon', 0
        Unknown2035: 8, s32'NotSearch_Icon', 0
      endIf: 
      else: 
        isObjectActive: (STACK4)
        if: Mem(Tmp)
          Unknown2035: 8, s32'Search_Icon', 0
        endIf: 
      endElse: 
    endElse: 
  endElse: 
  ifNot: Mem(BattleDoing)
    sendSignalToAction: s32'gri_Booster', 32
  endIf: 
endSubroutine: 

